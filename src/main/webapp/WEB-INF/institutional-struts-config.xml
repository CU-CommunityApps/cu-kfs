            <forward name="basic" path="/jsp/kim/IdentityManagementPersonDocument.jsp" />
            <forward name="inquiry" path="/jsp/kim/IdentityManagementPersonDocument.jsp" />
        </action>
        
    </action-mappings>
    
</struts-config>
