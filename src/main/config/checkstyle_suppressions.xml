<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN" "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>
	<!-- suppressing all checks on test code except a few...these checks are mostly error level - we don't want these errors even in test code -->
    <suppress checks="^((?!(EqualsHashCode|AnnotationUseStyle|CovariantEquals|DefaultComesLast|DoubleCheckedLocking|EqualsHashCode|PackageAnnotation|PackageDeclaration|StringLiteralEquality|SuperClone|SuperFinalize|UpperEll)).)*$"
    	files=".*[\\/]src[\\/]test[\\/]java[\\/].*"/>
</suppressions>