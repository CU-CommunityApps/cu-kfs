<?xml version="1.0" encoding="UTF-8"?>
<!--
    Default output definition for 1042S tax processing.
    This definition is for the tax year 2016 format.
 -->
<taxOutputDefinition xmlns="http://www.kuali.org/kfs/tax/taxOutputDefinition" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <section name="Biographic_Row"
             length="1890"
             hasHeaderRow="true"
             hasExactLength="false"
             hasSeparators="true"
             separator=",">
        <field name="First_Name" length="90" type="DERIVED" value="vendorFirstName" />
        <field name="Middle_Name" length="90" type="STATIC" value="" />
        <field name="Last_Name" length="90" type="DERIVED" value="vendorLastName" />
        <field name="Email" length="90" type="DERIVED" value="vendorEmailAddress" />
        <field name="Unique_ID_Student_Number" length="90" type="STATIC" value="" />
        <field name="TIN" length="90" type="DERIVED" value="ssn" />
        <field name="DOB" length="90" type="STATIC" value="" />
        <field name="Foreign_Tax_ID" length="90" type="DERIVED" value="itin" />
        <field name="Country_of_Residence" length="90" type="DERIVED" value="vendorForeignCountryCode" />
        <field name="US_Address_Line_1" length="90" type="DERIVED" value="vendorUSAddressLine1" />
        <field name="US_Address_Line_2" length="90" type="VENDOR_US_ADDRESS" value="vendorLine2Address" />
        <field name="US_City" length="90" type="VENDOR_US_ADDRESS" value="vendorCityName" />
        <field name="US_State" length="90" type="VENDOR_US_ADDRESS" value="vendorStateCode" />
        <field name="US_zip" length="90" type="VENDOR_US_ADDRESS" value="vendorZipCode" />
        <field name="NOnUS_Address_Line_1" length="90" type="DERIVED" value="vendorForeignAddressLine1" />
        <field name="NonUS_Address_Province" length="90" type="DERIVED" value="vendorForeignProvinceName" />
        <field name="NonUS_Address_City" length="90" type="DERIVED" value="vendorForeignCityName" />
        <field name="NonUS_address_Country" length="90" type="DERIVED" value="vendorForeignCountryCode" />
        <field name="NonUS-Address_postal_code" length="90" type="DERIVED" value="vendorForeignZipCode" />
        <field name="Mailing_Address" length="90" type="STATIC" value="" />
        <field name="Canada_Province" length="90" type="DERIVED" value="vendorForeignProvinceName" />
    </section>

</taxOutputDefinition>