<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" 
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans         
            http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="EntityAddress" parent="EntityAddress-parentBean">
        <property name="attributes">
            <list merge="true">
                <ref bean="EntityAddress-line1Unmasked"/>
                <ref bean="EntityAddress-line2Unmasked"/>
                <ref bean="EntityAddress-line3Unmasked"/>
                <ref bean="EntityAddress-cityUnmasked"/>
                <ref bean="EntityAddress-stateProvinceCodeUnmasked"/>
                <ref bean="EntityAddress-postalCodeUnmasked"/>
                <ref bean="EntityAddress-countryCodeUnmasked"/>
            </list>
        </property>
    </bean>

    <bean id="EntityAddress-line1Unmasked" parent="EntityAddress-line1Unmasked-parentBean"/>
    <bean id="EntityAddress-line1Unmasked-parentBean" abstract="true" parent="EntityAddress-line1"
          p:name="line1Unmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-GenericAttributeSecurity"/>

    <bean id="EntityAddress-line2Unmasked" parent="EntityAddress-line2Unmasked-parentBean"/>
    <bean id="EntityAddress-line2Unmasked-parentBean" abstract="true" parent="EntityAddress-line2"
          p:name="line2Unmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-GenericAttributeSecurity"/>

    <bean id="EntityAddress-line3Unmasked" parent="EntityAddress-line3Unmasked-parentBean"/>
    <bean id="EntityAddress-line3Unmasked-parentBean" abstract="true" parent="EntityAddress-line3"
          p:name="line3Unmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-GenericAttributeSecurity"/>

    <bean id="EntityAddress-cityUnmasked" parent="EntityAddress-cityUnmasked-parentBean"/>
    <bean id="EntityAddress-cityUnmasked-parentBean" abstract="true" parent="EntityAddress-city"
          p:name="cityUnmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-GenericAttributeSecurity"/>

    <bean id="EntityAddress-stateProvinceCodeUnmasked"
          parent="EntityAddress-stateProvinceCodeUnmasked-parentBean"/>
    <bean id="EntityAddress-stateProvinceCodeUnmasked-parentBean"
          abstract="true" parent="EntityAddress-stateProvinceCode"
          p:name="stateProvinceCodeUnmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-CodeAttributeSecurity"/>

    <bean id="EntityAddress-postalCodeUnmasked" parent="EntityAddress-postalCodeUnmasked-parentBean"/>
    <bean id="EntityAddress-postalCodeUnmasked-parentBean" abstract="true" parent="EntityAddress-postalCode"
          p:name="postalCodeUnmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-ZipAttributeSecurity"/>

    <bean id="EntityAddress-countryCodeUnmasked" parent="EntityAddress-countryCodeUnmasked-parentBean"/>
    <bean id="EntityAddress-countryCodeUnmasked-parentBean" abstract="true" parent="EntityAddress-countryCode"
          p:name="countryCodeUnmasked"
          p:attributeSecurity-ref="CuKimBaseBeans-CodeAttributeSecurity"/>

</beans>
