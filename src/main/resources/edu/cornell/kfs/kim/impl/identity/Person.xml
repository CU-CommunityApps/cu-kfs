            <list merge="true">
                <!-- Start of FINP-9357 backport section. -->
                <ref bean="Person-addressLine1MaskedIfNecessary"/>
                <ref bean="Person-addressLine2MaskedIfNecessary"/>
                <ref bean="Person-addressLine3MaskedIfNecessary"/>
                <ref bean="Person-addressCityMaskedIfNecessary"/>
                <ref bean="Person-addressStateProvinceCodeMaskedIfNecessary"/>
                <ref bean="Person-addressPostalCodeMaskedIfNecessary"/>
                <ref bean="Person-addressCountryCodeMaskedIfNecessary"/>
                <!-- End of FINP-9357 backport section. -->
                <ref bean="Person-firstNameMaskedIfNecessary"/>
                <ref bean="Person-lastNameMaskedIfNecessary"/>
                <ref bean="Person-middleNameMaskedIfNecessary"/>
                <ref bean="Person-nameMaskedIfNecessary"/>
                <ref bean="Person-emailAddressMaskedIfNecessary"/>
                <ref bean="Person-phoneNumberMaskedIfNecessary"/>
                <ref bean="Person-academicAffiliation"/>
                <ref bean="Person-affiliateAffiliation"/>
                <ref bean="Person-alumniAffiliation"/>
                <ref bean="Person-exceptionAffiliation"/>
                <ref bean="Person-facultyAffiliation"/>
                <ref bean="Person-staffAffiliation"/>
                <ref bean="Person-studentAffiliation"/>
                <ref bean="Person-altAddressTypeCode"/>
                <ref bean="Person-altAddressLine1"/>
                <ref bean="Person-altAddressLine1MaskedIfNecessary"/>
                <ref bean="Person-altAddressLine2"/>
                <ref bean="Person-altAddressLine2MaskedIfNecessary"/>
                <ref bean="Person-altAddressLine3"/>
                <ref bean="Person-altAddressLine3MaskedIfNecessary"/>
                <ref bean="Person-altAddressCity"/>
                <ref bean="Person-altAddressCityMaskedIfNecessary"/>
                <ref bean="Person-altAddressStateProvinceCode"/>
                <ref bean="Person-altAddressStateProvinceCodeMaskedIfNecessary"/>
                <ref bean="Person-altAddressPostalCode"/>
                <ref bean="Person-altAddressPostalCodeMaskedIfNecessary"/>
                <ref bean="Person-altAddressCountryCode"/>
                <ref bean="Person-altAddressCountryCodeMaskedIfNecessary"/>
                <ref bean="Person-suppressName"/>
                <ref bean="Person-suppressEmail"/>
                <ref bean="Person-suppressPhone"/>
                <ref bean="Person-suppressPersonal"/>
            </list>
        </property>
    </bean>

