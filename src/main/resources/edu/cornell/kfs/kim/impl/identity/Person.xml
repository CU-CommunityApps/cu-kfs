    <!-- Start of Person Inquiry overrides. -->

    <bean id="Person-inquiryDefinition" parent="Person-inquiryDefinition-parentBean"
          p:inquirableClass="edu.cornell.kfs.kim.inquiry.CuPersonInquirableImpl"/>

    <bean id="Person-FieldOverrideForInquirySectionInsert"
          abstract="true"
          parent="FieldOverrideForListElementInsert"
          p:propertyName="inquirySections"
          p:propertyNameForElementCompare="title"
    >
        <property name="element">
            <bean parent="InquirySectionDefinition"
                  p:title="Title"
                  p:numberOfColumns="1"/>
        </property>
    </bean>

    <bean id="Person-FieldOverrideForInquirySectionReplace"
          abstract="true"
          parent="FieldOverrideForListElementReplace"
          p:propertyName="inquirySections"
          p:propertyNameForElementCompare="title"
    >
        <property name="element">
            <bean parent="InquirySectionDefinition"
                  p:title="Title"
                  p:numberOfColumns="1"/>
        </property>
    </bean>

    <bean parent="DataDictionaryBeanOverride"
          p:beanName="Person-inquiryDefinition">
        <property name="fieldOverrides">
            <list>
                <!-- Start of FINP-9357 backport section. -->
                <bean parent="Person-FieldOverrideForInquirySectionReplace"
                      p:element.title="Address"
                >
                    <property name="replaceWith">
                        <bean parent="InquirySectionDefinition"
                              p:title="Address"
                              p:numberOfColumns="1"
                        >
                            <property name="inquiryFields">
                                <list>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressTypeCode"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressLine1MaskedIfNecessary"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressLine2MaskedIfNecessary"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressLine3MaskedIfNecessary"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressCityMaskedIfNecessary"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressStateProvinceCodeMaskedIfNecessary"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressPostalCodeMaskedIfNecessary"
                                          p:noInquiry="true"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="addressCountryCodeMaskedIfNecessary"
                                          p:noInquiry="true"/>
                                </list>
                            </property>
                        </bean>
                    </property>
                </bean>
                <!-- End of FINP-9357 backport section. -->
                <bean parent="Person-FieldOverrideForInquirySectionReplace"
                      p:element.title="Affiliation"
                >
                    <property name="replaceWith">
                        <bean parent="InquirySectionDefinition"
                              p:numberOfColumns="2"
                              p:title="Affiliation"
                        >
                            <property name="inquiryFields">
                                <list>
                                    <bean parent="InquirySubSectionHeaderDefinition"
                                          p:attributeName="Primary Affiliation"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="affiliationTypeCode"
                                          p:noInquiry="true"
                                    />
                                    <bean parent="FieldDefinition"
                                          p:attributeName="campusCode"
                                          p:noInquiry="true"
                                    />
                                    <bean parent="InquirySubSectionHeaderDefinition"
                                          p:attributeName="All Affiliations"/>
                                    <bean parent="FieldDefinition"
                                          p:attributeName="academicAffiliation"
                                          p:noInquiry="true"
                                    />
                                    <bean parent="FieldDefinition"
                                          p:attributeName="affiliateAffiliation"
                                          p:noInquiry="true"
