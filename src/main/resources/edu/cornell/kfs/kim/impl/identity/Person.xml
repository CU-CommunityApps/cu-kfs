            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--
        CU Customizations:
        
        * Added CU-specific Person fields.
        
        * Configured an alternate validation pattern for phone numbers.
        
        * Added masking of Base Salary Amount.
        
        * Backported the following KualiCo changes into this file:
        ** FINP-9357 (can be removed from this file when we upgrade to the 2023-02-22 financials patch)
        ** FINP-9235 (can be removed from this file when we upgrade to the 2023-03-08 financials patch)
     -->

    <import resource="classpath:edu/cornell/kfs/kim/cu-kim-attribute-beans.xml"/>
    <import resource="classpath:edu/cornell/kfs/kim/cu-kim-lookup-beans.xml"/>

    <!-- NOTE: The "searchService" property override is part of the FINP-9235 backport. -->
    <bean id="Person"
          parent="Person-parentBean"
          p:searchService-ref="personSearchService"
    >
