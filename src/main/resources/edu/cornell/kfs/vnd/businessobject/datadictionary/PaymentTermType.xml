<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <!-- Business Object Lookup Definition -->

    <bean id="PaymentTermType-lookupDefinition" parent="PaymentTermType-lookupDefinition-parentBean"/>
    <bean id="PaymentTermType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
          p:title="Payment Terms Type Lookup" p:lookupSearchService-ref="defaultLookupSearchService">
        <property name="lookupAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorPaymentTermsCode"/>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorPaymentTermsDescription"/>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorPaymentTermsPercent"/>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorDiscountDueNumber"/>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorDiscountDueTypeDescription"/>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorNetDueNumber"/>
                <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                      parent="PaymentTermType-vendorNetDueTypeDescription"/>
                <ref bean="activeIndicatorLookupAttributeDefinition"/>
            </list>
        </property>
        <property name="lookupResultAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorPaymentTermsCode"/>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorPaymentTermsDescription"/>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorPaymentTermsPercent" p:type="DECIMAL">
                    <property name="validationPattern">
                        <bean parent="FixedPointValidationPattern" p:scale="3" p:precision="6" />
                    </property>
                </bean>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorDiscountDueNumber"/>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorDiscountDueTypeDescription"/>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorNetDueNumber"/>
                <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                      parent="PaymentTermType-vendorNetDueTypeDescription"/>
                <ref bean="activeIndicatorLookupResultAttributeDefinition"/>
            </list>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="vendorPaymentTermsCode"/>
                <bean parent="FieldDefinition" p:attributeName="vendorPaymentTermsDescription"/>
                <bean parent="FieldDefinition" p:attributeName="vendorPaymentTermsPercent"/>
                <bean parent="FieldDefinition" p:attributeName="vendorDiscountDueNumber"/>
                <bean parent="FieldDefinition" p:attributeName="vendorDiscountDueTypeDescription"/>
                <bean parent="FieldDefinition" p:attributeName="vendorNetDueNumber"/>
                <bean parent="FieldDefinition" p:attributeName="vendorNetDueTypeDescription"
                      p:defaultValueFinder-ref="paymentTypeValuesFinder"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="vendorPaymentTermsCode"/>
                <bean parent="FieldDefinition" p:attributeName="vendorPaymentTermsDescription"/>
                <bean parent="FieldDefinition" p:attributeName="vendorPaymentTermsPercent"/>
                <bean parent="FieldDefinition" p:attributeName="vendorDiscountDueNumber"/>
                <bean parent="FieldDefinition" p:attributeName="vendorDiscountDueTypeDescription"/>
                <bean parent="FieldDefinition" p:attributeName="vendorNetDueNumber"/>
                <bean parent="FieldDefinition" p:attributeName="vendorNetDueTypeDescription"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>
    
</beans>