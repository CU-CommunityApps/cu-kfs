<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--
        CU Customization: Override Vendor Debarred bean to implement an equivalent of the FINP-11997 fix.
                          This override can be removed when we upgrade to the 2025-05-07 version of financials.
    -->
    <bean id="VendorContract-vendorDetail.vendorHeader.vendorDebarredIndicator"
          parent="VendorContract-vendorDetail.vendorHeader.vendorDebarredIndicator-parentBean"
          p:defaultValue=""/>

    <!--
        CU Customization: Use a field override bean to implement an equivalent of the FINP-12042 lookup fix.
                          This override can be removed when we upgrade to the 2025-04-30 version of financials.
    -->
    <bean parent="DataDictionaryBeanOverride"
          p:beanName="VendorContract-lookupDefinition">
       <property name="fieldOverrides">
           <list>
               <bean parent="FieldOverrideForListElementReplace"
                     p:propertyName="displayAttributeDefinitions"
                     p:propertyNameForElementCompare="name"
                     p:element-ref="VendorContract-vendorB2bIndicator">
                   <property name="replaceWith">
                        <bean parent="VendorContract-vendorB2bIndicator" p:type="BOOLEAN"/>
                   </property>
               </bean>
           </list>
       </property>
   </bean>

</beans>
