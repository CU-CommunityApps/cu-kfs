<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="PaymentWorksVendorGlobalDetail" parent="PaymentWorksVendorGlobalDetail-parentBean"/>
    <bean id="PaymentWorksVendorGlobalDetail-parentBean" abstract="true" parent="BusinessObjectEntry"
          p:businessObjectClass="edu.cornell.kfs.pmw.businessobject.PaymentWorksVendorGlobalDetail"
          p:objectLabel="PaymentWorks Vendor Global Detail">
        <property name="attributes">
            <list>
                <ref bean="PaymentWorksVendorGlobalDetail-id"/>
                <ref bean="PaymentWorksVendorGlobalDetail-pmwVendor-kfsVendorNumber"/>
                <ref bean="PaymentWorksVendorGlobalDetail-pmwVendor-pmwVendorRequestId"/>
                <ref bean="PaymentWorksVendorGlobalDetail-pmwVendor-requestingCompanyLegalName"/>
                <ref bean="PaymentWorksVendorGlobalDetail-pmwVendor-supplierUploadStatus"/>
            </list>
        </property>
    </bean>

    <bean id="PaymentWorksVendorGlobalDetail-id" parent="PaymentWorksVendorGlobalDetail-id-parentBean"/>
    <bean id="PaymentWorksVendorGlobalDetail-id-parentBean" abstract="true" parent="PaymentWorksVendor-id"/>

    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-kfsVendorNumber"
          parent="PaymentWorksVendorGlobalDetail-pmwVendor-kfsVendorNumber-parentBean"/>
    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-kfsVendorNumber-parentBean"
          abstract="true"
          parent="PaymentWorksVendor-kfsVendorNumber"
          p:name="pmwVendor.kfsVendorNumber"/>

    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-pmwVendorRequestId"
          parent="PaymentWorksVendorGlobalDetail-pmwVendor-pmwVendorRequestId-parentBean"/>
    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-pmwVendorRequestId-parentBean"
          abstract="true"
          parent="PaymentWorksVendor-pmwVendorRequestId"
          p:name="pmwVendor.pmwVendorRequestId"/>

    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-requestingCompanyLegalName"
          parent="PaymentWorksVendorGlobalDetail-pmwVendor-requestingCompanyLegalName-parentBean"/>
    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-requestingCompanyLegalName-parentBean"
          abstract="true"
          parent="PaymentWorksVendor-requestingCompanyLegalName"
          p:name="pmwVendor.requestingCompanyLegalName"/>

    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-supplierUploadStatus"
          parent="PaymentWorksVendorGlobalDetail-pmwVendor-supplierUploadStatus-parentBean"/>
    <bean id="PaymentWorksVendorGlobalDetail-pmwVendor-supplierUploadStatus-parentBean"
          abstract="true"
          parent="PaymentWorksVendor-supplierUploadStatus"
          p:name="pmwVendor.supplierUploadStatus"/>

</beans>
