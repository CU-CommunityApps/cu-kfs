<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="IndirectCostRecoveryExclusionType" parent="IndirectCostRecoveryExclusionType-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-parentBean" abstract="true" parent="BusinessObjectEntry"
          p:name="IndirectCostRecoveryExclusionType"
          p:businessObjectClass="org.kuali.kfs.coa.businessobject.IndirectCostRecoveryExclusionType"
          p:businessObjectAdminService-ref="noActionsBoAdminService"
          p:inquiryDefinition-ref="IndirectCostRecoveryExclusionType-inquiryDefinition"
          p:lookupDefinition-ref="IndirectCostRecoveryExclusionType-lookupDefinition"
          p:objectLabel="Indirect Cost Recovery Exclusion By Type">
        <property name="attributes">
            <list>
                <ref bean="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode"/>
                <ref bean="IndirectCostRecoveryExclusionType-chartOfAccountsCode"/>
                <ref bean="IndirectCostRecoveryExclusionType-financialObjectCode"/>
                <ref bean="IndirectCostRecoveryExclusionType-active"/>
                <bean parent="GenericAttributes-newCollectionRecord"/>
            </list>
        </property>
        <property name="inactivationBlockingDefinitions">
            <list>
                <bean parent="InactivationBlockingDefinition" p:blockedReferencePropertyName="objectCodeCurrent"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode"
          parent="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode-parentBean" abstract="true"
          parent="IndirectCostRecoveryType-code" p:forceUppercase="true" p:name="accountIndirectCostRecoveryTypeCode"
          p:label="Type Code" p:shortLabel="Type" p:maxLength="2"/>

    <bean id="IndirectCostRecoveryExclusionType-chartOfAccountsCode"
          parent="IndirectCostRecoveryExclusionType-chartOfAccountsCode-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-chartOfAccountsCode-parentBean" abstract="true"
          parent="Chart-chartOfAccountsCode" p:validationPattern-ref="AnyCharacterValidation"/>

    <bean id="IndirectCostRecoveryExclusionType-financialObjectCode"
          parent="IndirectCostRecoveryExclusionType-financialObjectCode-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-financialObjectCode-parentBean" abstract="true"
          parent="ObjectCode-financialObjectCode" p:validationPattern-ref="AnyCharacterValidation"/>

    <bean id="IndirectCostRecoveryExclusionType-active" parent="IndirectCostRecoveryExclusionType-active-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-active-parentBean" abstract="true"
          parent="GenericAttributes-activeIndicator" p:name="active"/>

    <!-- Business Object Inquiry Definition -->

    <bean id="IndirectCostRecoveryExclusionType-inquiryDefinition"
          parent="IndirectCostRecoveryExclusionType-inquiryDefinition-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-inquiryDefinition-parentBean" abstract="true"
          parent="InquiryDefinition" p:title="Indirect Cost Recovery Exclusion By Type Inquiry">
        <property name="inquirySections">
            <list>
                <ref bean="IndirectCostRecoveryExclusionType-inquirySectionDefinition"/>
            </list>
        </property>
    </bean>

    <bean id="IndirectCostRecoveryExclusionType-inquirySectionDefinition"
          parent="IndirectCostRecoveryExclusionType-inquirySectionDefinition-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-inquirySectionDefinition-parentBean"
          parent="InquirySectionDefinition" abstract="true" p:title="" p:numberOfColumns="1">
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="accountIndirectCostRecoveryTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
                <bean parent="FieldDefinition" p:attributeName="objectCodeCurrent.financialObjectCode"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->

    <bean id="IndirectCostRecoveryExclusionType-lookupDefinition"
          parent="IndirectCostRecoveryExclusionType-lookupDefinition-parentBean"/>
    <bean id="IndirectCostRecoveryExclusionType-lookupDefinition-parentBean" abstract="true"
          parent="LookupDefinition" p:title="Indirect Cost Recovery Exclusion By Type Lookup"
          p:lookupSearchService-ref="defaultLookupSearchService"
          p:lookupableID="indirectCostRecoveryExclusionByTypeLookupable">
        <property name="defaultSort">
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>accountIndirectCostRecoveryTypeCode</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode" />
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-chartOfAccountsCode" />
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-financialObjectCode" />
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-active"
                      p:control-ref="GenericAttributes-genericBooleanYNBoth-lookupControl" p:defaultValue="Y"/>
            </list>
        </property>
        <property name="lookupResultAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-accountIndirectCostRecoveryTypeCode"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-chartOfAccountsCode"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-financialObjectCode"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="IndirectCostRecoveryExclusionType-active"/>
            </list>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="accountIndirectCostRecoveryTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
                <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
                <bean parent="FieldDefinition" p:defaultValue="Y" p:attributeName="active"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="accountIndirectCostRecoveryTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
                <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>
</beans>
