<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	<bean id="SubObjectCodeGlobalInactivationSearch" parent="SubObjectCodeGlobalInactivationSearch-parentBean" />

  <bean id="SubObjectCodeGlobalInactivationSearch-parentBean" abstract="true" parent="FinancialSystemBusinessObjectEntry">
    <property name="businessObjectClass" value="edu.cornell.kfs.coa.businessobject.SubObjectCodeGlobalInactivationSearch"/>
    <property name="inquiryDefinition">
      <ref bean="SubObjectCodeGlobalInactivationSearch-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="SubObjectCodeGlobalInactivationSearch-lookupDefinition"/>
    </property>
    <property name="titleAttribute" value="financialSubObjectCode"/>
    <property name="objectLabel" value="Sub-Object Code"/>
    <property name="attributes">
      <list>
        <bean parent="ObjectCode-universityFiscalYear"/>
        <bean parent="ObjectCode-chartOfAccountsCode"/>
        <bean parent="ObjectCode-financialObjectCode"/>
        <bean parent="Account-accountNumber"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-financialSubObjectCode"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-financialSubObjectCodeName"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-financialSubObjectCdshortNm"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-active"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevelCode"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevel.financialConsolidationObjectCode"/>
        <ref bean="SubObjectCodeGlobalInactivationSearch-account.organizationCode"/>
      </list>
    </property>
    <property name="accessRestrictedAttributes">
       <list>
           <bean parent="ChartRestriction"><property name="attribute" ref="ObjectCode-chartOfAccountsCode"/></bean>
           <bean parent="ChartHierarchyRestriction"><property name="attribute" ref="ObjectCode-chartOfAccountsCode"/></bean>
           <bean parent="AccountRestriction"><property name="attribute" ref="Account-accountNumber"/></bean>
           <bean parent="PrincipalInvestigatorRestriction"><property name="attribute" ref="Account-accountNumber"/></bean>
           <bean parent="ObjectLevelRestriction"><property name="attribute" ref="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevelCode"/></bean>
           <bean parent="ObjectConsolidationRestriction"><property name="attribute" ref="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevel.financialConsolidationObjectCode"/></bean>
           <bean parent="OrganizationRestriction"><property name="attribute" ref="SubObjectCodeGlobalInactivationSearch-account.organizationCode"/></bean>
           <bean parent="OrganizationHierarchyRestriction"><property name="attribute" ref="SubObjectCodeGlobalInactivationSearch-account.organizationCode"/></bean>
       </list>
    </property>
  </bean>

	<!-- Attribute Definitions -->

	<bean id="SubObjectCodeGlobalInactivationSearch-financialSubObjectCode" parent="SubObjectCodeGlobalInactivationSearch-financialSubObjectCode-parentBean" />
	<bean id="SubObjectCodeGlobalInactivationSearch-financialSubObjectCode-parentBean"
		abstract="true" parent="SubObjectAttribute" />

	<bean id="SubObjectCodeGlobalInactivationSearch-financialSubObjectCodeName" parent="SubObjectCodeGlobalInactivationSearch-financialSubObjectCodeName-parentBean" />
	<bean id="SubObjectCodeGlobalInactivationSearch-financialSubObjectCodeName-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="financialSubObjectCodeName" />
		<property name="label" value="Sub-Object Code Name" />
		<property name="shortLabel" value="SubObjCodeName" />
		<property name="maxLength" value="40" />
		<property name="validationPattern">
			<ref bean="AnyCharacterWithWhitespaceValidation" />
		</property>
		<property name="required" value="true" />
		<property name="control">
			<ref bean="StandardNameTextControl" />
		</property>
	</bean>

	<bean id="SubObjectCodeGlobalInactivationSearch-financialSubObjectCdshortNm" parent="SubObjectCodeGlobalInactivationSearch-financialSubObjectCdshortNm-parentBean" />
	<bean id="SubObjectCodeGlobalInactivationSearch-financialSubObjectCdshortNm-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="financialSubObjectCdshortNm" />
		<property name="label" value="Sub-Object Code Short Name" />
		<property name="shortLabel" value="SubObjCodeShortName" />
		<property name="maxLength" value="12" />
		<property name="validationPattern">
			<ref bean="AnyCharacterWithWhitespaceValidation" />
		</property>
		<property name="required" value="true" />
		<property name="control">
			<ref bean="StandardShortNameTextControl" />
		</property>
	</bean>

	<bean id="SubObjectCodeGlobalInactivationSearch-active" parent="SubObjectCodeGlobalInactivationSearch-active-parentBean" />
	<bean id="SubObjectCodeGlobalInactivationSearch-active-parentBean" abstract="true"
		parent="GenericAttributes-activeIndicator">
		<property name="name" value="active" />
	</bean>

    <bean id="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevelCode" parent="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevelCode-parentBean"/>
    <bean id="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevelCode-parentBean" abstract="true" parent="ObjectLevel-financialObjectLevelCode">
         <property name="name" value="financialObject.financialObjectLevelCode"/>
    </bean>
        
    <bean id="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevel.financialConsolidationObjectCode" parent="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevel.financialConsolidationObjectCode-parentBean"/>
    <bean id="SubObjectCodeGlobalInactivationSearch-objectCode.financialObjectLevel.financialConsolidationObjectCode-parentBean" abstract="true" parent="ObjectConsolidation-finConsolidationObjectCode">
         <property name="name" value="financialObject.financialObjectLevel.financialConsolidationObjectCode"/>
    </bean>
   
    <bean id="SubObjectCodeGlobalInactivationSearch-account.organizationCode" parent="SubObjectCodeGlobalInactivationSearch-account.organizationCode-parentBean"/>
    <bean id="SubObjectCodeGlobalInactivationSearch-account.organizationCode-parentBean" abstract="true" parent="Organization-organizationCode">
         <property name="name" value="account.organizationCode"/>
    </bean>                                               

<!-- Business Object Inquiry Definition -->


	<bean id="SubObjectCodeGlobalInactivationSearch-inquiryDefinition" parent="SubObjectCodeGlobalInactivationSearch-inquiryDefinition-parentBean" />

	<bean id="SubObjectCodeGlobalInactivationSearch-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="title" value="Sub-Object Code Inquiry" />
		<property name="inquirySections">
			<list>
				<ref bean="SubObjectCodeGlobalInactivationSearch-inquirySectionDefinition" />
			</list>
		</property>
	</bean>

	<bean id="SubObjectCodeGlobalInactivationSearch-inquirySectionDefinition" parent="SubObjectCodeGlobalInactivationSearch-inquirySectionDefinition-parentBean" />
	<bean id="SubObjectCodeGlobalInactivationSearch-inquirySectionDefinition-parentBean"
		parent="InquirySectionDefinition" abstract="true">
		<property name="title" value="" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="universityFiscalYear" />
				<bean parent="FieldDefinition" p:attributeName="accountNumber" />
				<bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode" />
				<bean parent="FieldDefinition" p:attributeName="financialObjectCode" />
				<bean parent="FieldDefinition" p:attributeName="financialSubObjectCode" />
				<bean parent="FieldDefinition" p:attributeName="financialSubObjectCodeName" />
				<bean parent="FieldDefinition" p:attributeName="active" />
				<bean parent="FieldDefinition" p:attributeName="financialSubObjectCdshortNm" />
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->

  <bean id="SubObjectCodeGlobalInactivationSearch-lookupDefinition" parent="SubObjectCodeGlobalInactivationSearch-lookupDefinition-parentBean" />
  <bean id="SubObjectCodeGlobalInactivationSearch-lookupDefinition-parentBean" abstract="true" parent="SubObjectCodeGlobalInactivationSearch-lookupDefinition-base-parentBean" />
  <bean id="SubObjectCodeGlobalInactivationSearch-lookupDefinition-base-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Sub-Object Code Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>financialSubObjectCode</value>
          </list>
        </property>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear" p:defaultValueFinderClass="org.kuali.kfs.sys.businessobject.defaultvalue.FiscalYearFinder"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCodeName"/>
        <bean parent="FieldDefinition" p:attributeName="account.organizationCode"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="accountNumber"/>
        <bean parent="FieldDefinition" p:attributeName="financialObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="financialSubObjectCodeName"/>
        <bean parent="FieldDefinition" p:attributeName="account.organizationCode"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
  </bean>
</beans>
