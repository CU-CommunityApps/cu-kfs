<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

    <bean id="KualiAddressMaintenanceDocument" parent="KualiAddressMaintenanceDocument-parentBean"/>
    <bean id="KualiAddressMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
        <property name="businessObjectClass" value="edu.cornell.kfs.sys.businessobject.KualiAddress"/>
        <property name="maintainableClass" value="org.kuali.kfs.sys.document.FinancialSystemMaintainable"/>
        <property name="maintainableSections">
            <list>
                <ref bean="KualiAddressMaintenanceDocument-EditKualiAddress"/>
            </list>
        </property>
    
        <property name="documentTypeName" value="ADDR"/>
        <property name="documentAuthorizerClass" value="org.kuali.kfs.sys.document.authorization.FinancialSystemMaintenanceDocumentAuthorizerBase"/>
    
        <property name="workflowAttributes">
            <ref bean="KualiAddressMaintenanceDocument-workflowAttributes"/>
        </property>
    </bean>

    <!-- Maintenance Section Definitions -->
    <bean id="KualiAddressMaintenanceDocument-EditKualiAddress" parent="KualiAddressMaintenanceDocument-EditKualiAddress-parentBean"/>
    <bean id="KualiAddressMaintenanceDocument-EditKualiAddress-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Kuali Address"/>
        <property name="title" value="Edit Kuali Address"/>
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="addressId" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="addressTypeCode" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="streetAddress" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="streetAddressExtra" p:required="false"/>
                <bean parent="MaintainableFieldDefinition" p:name="city" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="stateCode" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="zipCode" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="countryCode" p:required="true" p:defaultValue="US" />
                <bean parent="MaintainableFieldDefinition" p:name="active" p:required="true" p:defaultValue="Y" />
            </list>
        </property>
    </bean>

    <!-- workflow attributes -->
    <bean id="KualiAddressMaintenanceDocument-workflowAttributes" parent="KualiAddressMaintenanceDocument-workflowAttributes-parentBean"/>
    <bean id="KualiAddressMaintenanceDocument-workflowAttributes-parentBean" class="org.kuali.kfs.krad.datadictionary.WorkflowAttributes" abstract="true"/>

</beans>