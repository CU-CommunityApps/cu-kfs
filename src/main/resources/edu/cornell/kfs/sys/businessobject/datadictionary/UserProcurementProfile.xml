    <property name="forceUppercase" value="true"/>
    <property name="label" value="Document Type Code"/>
    <property name="shortLabel" value="Document Type"/>
    <property name="required" value="true"/>
    <property name="maxLength" value="64"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="6"/>
    </property>
    
    
  </bean>
 -->
  <bean id="UserProcurementProfile-principalId" parent="UserProcurementProfile-principalId-parentBean"/>

  <bean id="UserProcurementProfile-principalId-parentBean"  abstract="true" parent="PersonImpl-principalId">
    <property name="name" value="principalId"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Person Universal Identifier"/>
    <property name="shortLabel" value="Identifier"/>
    <property name="maxLength" value="10"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterValidation" />
    </property>
    <property name="control">
    <!--       <bean parent="TextControlDefinition" p:size="10"/>
    </property>
 
    <property name="control">-->
      <bean parent="HiddenControlDefinition"/>
    </property>
    <property name="displayLabelAttribute" value="profileUser.principalName"/>
  
    
  </bean>
  
        <bean id="UserProcurementProfile-profileUser.principalName" parent="UserProcurementProfile-profileUser.principalName-parentBean"/>

  <dd:boAttributeRef id="UserProcurementProfile-profileUser.principalName-parentBean" parent="PersonImpl-principalName" abstract="true" 
  attributeName="profileUser.principalName" label="User Principal Name" shortLabel="Principal Name" required="false">
    <dd:user universalIdAttribute="principalId" userObjectAttribute="profileUser"/>
  </dd:boAttributeRef>
    
    <bean id="UserProcurementProfile-profileUser.name" parent="UserProcurementProfile-profileUser.name-parentBean"/>

  <dd:boAttributeRef id="UserProcurementProfile-profileUser.name-parentBean" parent="PersonImpl-name" abstract="true" 
  attributeName="profileUser.name" label="Person Name" shortLabel="Person Name" required="false">
    <dd:user universalIdAttribute="principalId" userObjectAttribute="profileUser"/>
  </dd:boAttributeRef>
  
  <bean id="UserProcurementProfile-active" parent="UserProcurementProfile-active-parentBean"/>
  <bean id="UserProcurementProfile-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active"/>
    <property name="shortLabel" value="User Profile Active"/>
  </bean>  
  
  <bean id="UserProcurementProfile-favoriteAccounts.chartOfAccountsCode" parent="UserProcurementProfile-favoriteAccounts.chartOfAccountsCode-parentBean"/>

  <bean id="UserProcurementProfile-favoriteAccounts.chartOfAccountsCode-parentBean" abstract="true" parent="FavoriteAccount-chartOfAccountsCode">
