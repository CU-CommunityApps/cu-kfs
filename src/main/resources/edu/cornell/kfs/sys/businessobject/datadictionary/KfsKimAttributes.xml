<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <import resource="classpath:org/kuali/kfs/coa/coa-lookup-beans.xml"/>
    <import resource="classpath:org/kuali/kfs/vnd/vnd-lookup-beans.xml"/>

    <bean id="KfsKimAttributes" parent="KfsKimAttributes-parentBean"/>
    <bean id="KfsKimAttributes-parentBean" abstract="true" parent="KimAttributes-parentBean"
          p:businessObjectClass="org.kuali.kfs.sys.identity.KfsKimAttributes" p:objectLabel="KFS Core KIM Attributes">
        <property name="attributes">
            <list merge="true">
                <ref bean="KfsKimAttributes-chartOfAccountsCode"/>
                <ref bean="KfsKimAttributes-accountNumber"/>
                <ref bean="KfsKimAttributes-organizationCode"/>
                <ref bean="KfsKimAttributes-descendHierarchy"/>
                <ref bean="KfsKimAttributes-fromAmount"/>
                <ref bean="KfsKimAttributes-toAmount"/>
                <ref bean="KfsKimAttributes-accountingLineOverrideCode"/>
                <ref bean="KfsKimAttributes-subFundGroupCode"/>
                <ref bean="KfsKimAttributes-purchasingCommodityCode"/>
                <ref bean="KfsKimAttributes-contractManagerCode"/>
                <ref bean="KfsKimAttributes-customerProfileId"/>
                <ref bean="KfsKimAttributes-achTransactionTypeCode"/>
                <ref bean="KfsKimAttributes-vendorTypeCode"/>
                <ref bean="KfsKimAttributes-contractsAndGrantsAccountResponsibilityId"/>
                <ref bean="KfsKimAttributes-paymentMethodCode"/>
                <ref bean="KfsKimAttributes-subAccountNumber"/>
                <ref bean="KfsKimAttributes-filePath"/>
                <ref bean="KfsKimAttributes-profilePrincipalId"/>
                <ref bean="KfsKimAttributes-reportCode"/>
                <ref bean="KfsKimAttributes-campusCode"/>
            </list>
        </property>
        <property name="relationships">
            <list merge="true">
                <bean parent="RelationshipDefinition" p:objectAttributeName="chart">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="chartOfAccountsCode"
                                  p:targetName="chartOfAccountsCode"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="account">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="chartOfAccountsCode"
                                  p:targetName="chartOfAccountsCode"/>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="accountNumber"
                                  p:targetName="accountNumber"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="subAccount">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="chartOfAccountsCode"
                                  p:targetName="chartOfAccountsCode"/>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="accountNumber"
                                  p:targetName="accountNumber"/>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="subAccountNumber"
                                  p:targetName="subAccountNumber"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="organization">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="chartOfAccountsCode"
                                  p:targetName="chartOfAccountsCode"/>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="organizationCode"
                                  p:targetName="organizationCode"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="subFundGroup">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="subFundGroupCode"
                                  p:targetName="subFundGroupCode"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="contractManager">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="contractManagerCode"
                                  p:targetName="contractManagerCode"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="commodityCode">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="purchasingCommodityCode"
                                  p:targetName="purchasingCommodityCode"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="customerProfile">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="customerProfileId"
                                  p:targetName="id"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition" p:objectAttributeName="vendorType">
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="vendorTypeCode"
                                  p:targetName="vendorTypeCode"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="KfsKimAttributes-chartOfAccountsCode" parent="Chart-chartOfAccountsCode">
        <property name="control">
            <bean parent="SelectControlDefinition" p:valuesFinder-ref="chartValuesFinder"
                  p:includeKeyInLabel="true"/>
        </property>
    </bean>

    <bean id="KfsKimAttributes-accountNumber" parent="Account-accountNumber"/>

    <bean id="KfsKimAttributes-organizationCode" parent="Organization-organizationCode"/>

    <bean id="KfsKimAttributes-accountingLineOverrideCode"
          parent="KfsKimAttributes-accountingLineOverrideCode-parentBean"/>
    <bean id="KfsKimAttributes-accountingLineOverrideCode-parentBean" abstract="true"
          parent="SourceAccountingLine-overrideCode" p:name="accountingLineOverrideCode"
          p:label="Accounting Line Override Code" p:shortLabel="Override Code"/>

    <bean id="KfsKimAttributes-subFundGroupCode" parent="SubFundGroup-subFundGroupCode"/>
    <bean id="KfsKimAttributes-purchasingCommodityCode" parent="CommodityCode-purchasingCommodityCode"
          p:required="true"/>

    <bean id="KfsKimAttributes-contractManagerCode" parent="ContractManager-contractManagerCode" p:required="true"/>

    <bean id="KfsKimAttributes-customerProfileId" parent="KfsKimAttributes-customerProfileId-parentBean"/>
    <bean id="KfsKimAttributes-customerProfileId-parentBean" abstract="true" parent="CustomerProfile-id"
          p:name="customerProfileId" p:label="Customer Profile Id" p:shortLabel="Id" p:required="false"/>

    <bean id="KfsKimAttributes-achTransactionTypeCode" parent="KfsKimAttributes-achTransactionTypeCode-parentBean"/>
    <bean id="KfsKimAttributes-achTransactionTypeCode-parentBean" abstract="true"
          parent="PayeeACHAccount-achTransactionType" p:name="achTransactionTypeCode" p:required="false">
        <property name="control">
            <bean parent="SelectControlDefinition" p:valuesFinder-ref="achTransactionTypeCodeValuesFinder"
                  p:includeKeyInLabel="true"/>
        </property>
    </bean>

    <bean id="KfsKimAttributes-vendorTypeCode" parent="VendorType-vendorTypeCode" p:required="true">
        <property name="control">
            <bean parent="SelectControlDefinition" p:valuesFinder-ref="vendorTypeValuesFinder"
                  p:includeKeyInLabel="true"/>
        </property>
    </bean>

    <bean id="KfsKimAttributes-contractsAndGrantsAccountResponsibilityId"
          parent="Account-contractsAndGrantsAccountResponsibilityId"/>

    <bean id="KfsKimAttributes-paymentMethodCode" parent="KfsKimAttributes-paymentMethodCode-parentBean"/>
    <bean id="KfsKimAttributes-paymentMethodCode-parentBean" abstract="true"
          parent="DisbursementVoucherDocument-disbVchrPaymentMethodCode"
          p:name="paymentMethodCode" p:required="true"/>

    <bean id="KfsKimAttributes-subAccountNumber" parent="SubAccount-subAccountNumber"/>

    <bean id="KfsKimAttributes-fromAmount" parent="KfsKimAttributes-fromAmount-parentBean"/>
    <bean id="KfsKimAttributes-fromAmount-parentBean" abstract="true"
          parent="FinancialSystemDocumentHeader-financialDocumentTotalAmount" p:name="fromAmount"
          p:label="From Amount" p:shortLabel="From Amount"/>

    <bean id="KfsKimAttributes-toAmount" parent="KfsKimAttributes-toAmount-parentBean"/>
    <bean id="KfsKimAttributes-toAmount-parentBean" abstract="true"
          parent="FinancialSystemDocumentHeader-financialDocumentTotalAmount" p:name="toAmount" p:label="To Amount"
          p:shortLabel="To Amount"/>

    <bean id="KfsKimAttributes-descendHierarchy" parent="KfsKimAttributes-descendHierarchy-parentBean"/>
    <bean id="KfsKimAttributes-descendHierarchy-parentBean" abstract="true" parent="AttributeDefinition"
          p:name="descendHierarchy" p:label="Descends Hierarchy" p:shortLabel="Descends Hierarchy"
          p:required="true" p:forceUppercase="true" p:maxLength="1" p:control-ref="IndicatorYNSelectControl"/>

    <bean id="KfsKimAttributes-filePath" parent="BatchFile-path" p:name="filePath" p:label="File Path"
          p:shortLabel="File Path"/>

    <bean id="KfsKimAttributes-profilePrincipalId" parent="KfsKimAttributes-profilePrincipalId-parentBean"/>
    <bean id="KfsKimAttributes-profilePrincipalId-parentBean" abstract="true" parent="AttributeDefinition"
          p:name="profilePrincipalId" p:label="Profile Principal Id">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="20"/>
        </property>
    </bean>

    <bean id="KfsKimAttributes-reportCode" parent="KfsKimAttributes-reportCode-parentBean"/>
    <bean id="KfsKimAttributes-reportCode-parentBean" abstract="true" parent="AttributeDefinition"
          p:name="reportCode" p:label="Report Code" p:shortLabel="Report Code">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="20"/>
        </property>
    </bean>

    <bean id="KfsKimAttributes-campusCode" parent="KfsKimAttributes-campusCode-parentBean"/>
    <bean id="KfsKimAttributes-campusCode-parentBean" abstract="true" parent="Campus-code" p:name="campusCode"/>

    <bean id="achTransactionTypeCodeValuesFinder"
          class="org.kuali.kfs.pdp.businessobject.options.AchTransactionTypeCodeValuesFinder"
          p:keyValuesService-ref="keyValuesService"/>
</beans>
