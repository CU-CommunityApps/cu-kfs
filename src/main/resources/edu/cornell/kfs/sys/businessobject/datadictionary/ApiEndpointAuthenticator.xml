<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="ApiEndpointAuthenticator" parent="ApiEndpointAuthenticator-parentBean"/>

    <bean id="ApiEndpointAuthenticator-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="edu.cornell.kfs.sys.businessobject.ApiEndpointAuthenticator"/>
        <property name="objectLabel" value="API Endpoint Authenticator"/>
        <property name="titleAttribute" value="id"/>

        <property name="inquiryDefinition">
            <ref bean="ApiEndpointAuthenticator-inquiryDefinition"/>
        </property>
        <property name="lookupDefinition">
            <ref bean="ApiEndpointAuthenticator-lookupDefinition"/>
        </property>
        <property name="attributes">
            <list>
                <ref bean="ApiEndpointAuthenticator-id"/>
                <ref bean="ApiEndpointAuthenticator-authenticatorId"/>
                <ref bean="ApiEndpointAuthenticator-endpointCode"/>
                <ref bean="ApiEndpointAuthenticator-active"/>
            </list>
        </property>
        <property name="relationships">
            <list>
                <bean parent="RelationshipDefinition">
                    <property name="objectAttributeName" value="apiEndpointDescription"/>
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="endpointCode" p:targetName="endpointCode"/>
                        </list>
                    </property>
                </bean>
                <bean parent="RelationshipDefinition">
                    <property name="objectAttributeName" value="apiAuthenticator"/>
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="authenticatorId" p:targetName="authenticatorId"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ApiEndpointAuthenticator-id" parent="ApiEndpointAuthenticator-id-parentBean"/>

    <bean id="ApiEndpointAuthenticator-id-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="id"/>
        <property name="label" value="Description-Autenticator-ID"/>
        <property name="shortLabel" value="Description-Autenticator-ID"/>
        <property name="maxLength" value="10"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="12"/>
        </property>
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="attributeSecurity">
            <bean parent="AttributeSecurity" p:readOnly="false"/>
        </property>
    </bean>

    <bean id="ApiEndpointAuthenticator-authenticatorId" parent="ApiEndpointAuthenticator-authenticatorId-parentBean"/>

    <bean id="ApiEndpointAuthenticator-authenticatorId-parentBean" abstract="true" parent="ApiAuthenticator-authenticatorId"/>

    <bean id="ApiEndpointAuthenticator-endpointCode" parent="ApiEndpointAuthenticator-endpointCode-parentBean"/>

    <bean id="ApiEndpointAuthenticator-endpointCode-parentBean" abstract="true" parent="ApiEndpointDescription-endpointCode"/>

    <bean id="ApiEndpointAuthenticator-active" parent="ApiEndpointAuthenticator-active-parentBean"/>
    <bean id="ApiEndpointAuthenticator-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
        <property name="name" value="active"/>
    </bean>

    <bean id="ApiEndpointAuthenticator-inquiryDefinition" parent="ApiEndpointAuthenticator-inquiryDefinition-parentBean"/>

    <bean id="ApiEndpointAuthenticator-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="API Endpoint Authenticator Inquiry"/>
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="API Endpoint Authenticator Inquiry"/>
                    <property name="numberOfColumns" value="1"/>
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id"/>
                            <bean parent="FieldDefinition" p:attributeName="authenticatorId"/>
                            <bean parent="FieldDefinition" p:attributeName="endpointCode"/>
                            <bean parent="FieldDefinition" p:attributeName="active"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="ApiEndpointAuthenticator-lookupDefinition" parent="ApiEndpointAuthenticator-lookupDefinition-parentBean"/>

    <bean id="ApiEndpointAuthenticator-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="API Endpoint Authenticator Lookup"/>

        <property name="defaultSort" >
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>id</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id"/>
                <bean parent="FieldDefinition" p:attributeName="authenticatorId"/>
                <bean parent="FieldDefinition" p:attributeName="endpointCode"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id"/>
                <bean parent="FieldDefinition" p:attributeName="authenticatorId"/>
                <bean parent="FieldDefinition" p:attributeName="endpointCode"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>

</beans>
