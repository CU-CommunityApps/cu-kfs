<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">

    <bean id="KualiDeveloper" parent="KualiDeveloper-parentBean"/>
	<bean id="KualiDeveloper-parentBean" abstract="true" parent="BusinessObjectEntry">
	    <property name="businessObjectClass" value="edu.cornell.kfs.sys.businessobject.KualiDeveloper"/>
	    <property name="objectLabel" value="KualiDeveloper"/>
        <property name="inquiryDefinition">
            <ref bean="KualiDeveloper-inquiryDefinition"/>
        </property>
        <property name="lookupDefinition">
            <ref bean="KualiDeveloper-lookupDefinition"/>
        </property>
        <property name="titleAttribute" value="employeeId"/>
		<property name="attributes">
            <list>
                <ref bean="KualiDeveloper-firstName"/>
                <ref bean="KualiDeveloper-lastName"/>
                <ref bean="KualiDeveloper-positionName"/>
                <ref bean="KualiDeveloper-socialSecurityNumber"/>
                <ref bean="KualiDeveloper-employeeId"/>
                <ref bean="KualiDeveloper-active"/>
                <ref bean="KualiDeveloper-kualiAddresses.addressTypeCode"/>
                <ref bean="KualiDeveloper-kualiAddresses.streetAddress"/>
                <ref bean="KualiDeveloper-kualiAddresses.streetAddressExtra"/>
                <ref bean="KualiDeveloper-kualiAddresses.city"/>
                <ref bean="KualiDeveloper-kualiAddresses.stateCode"/>
                <ref bean="KualiDeveloper-kualiAddresses.zipCode"/>
                <ref bean="KualiDeveloper-kualiAddresses.countryCode"/>
            </list>
		</property>
        <property name="collections">
            <list>
                <bean parent="CollectionDefinition" p:name="kualiAddresses"
                      p:label="Developer Addresses" p:shortLabel="Addresses" p:elementLabel="Developer Address"/>
            </list>            
        </property>
	</bean>
            
    <bean id="KualiDeveloper-firstName" parent="KualiDeveloper-firstName-parentBean"/>
    <bean id="KualiDeveloper-firstName-parentBean" abstract="true" parent="AttributeDefinition">
	    	<property name="name" value="firstName"/>
	    	<property name="label" value="First Name"/>
	    	<property name="shortLabel" value="First Name"/>
	    	<property name="maxLength" value="64"/>
	    	<property name="validationPattern">
      		<bean parent="AlphaValidationPattern" />
	    	</property>
	    	<property name="control">
	    	  	<bean parent="TextControlDefinition" p:size="12"/>
	    	</property>
    </bean>
            
    <bean id="KualiDeveloper-lastName" parent="KualiDeveloper-lastName-parentBean"/>
    <bean id="KualiDeveloper-lastName-parentBean" abstract="true" parent="AttributeDefinition">
	    	<property name="name" value="lastName"/>
	    	<property name="label" value="Last Name"/>
	    	<property name="shortLabel" value="Last Name"/>
	    	<property name="maxLength" value="64"/>
	    	<property name="validationPattern">
      		<bean parent="AlphaValidationPattern" />
	    	</property>
	    	<property name="control">
	    	  	<bean parent="TextControlDefinition" p:size="12"/>
	    	</property>
    </bean>
            
    <bean id="KualiDeveloper-positionName" parent="KualiDeveloper-positionName-parentBean"/>
    <bean id="KualiDeveloper-positionName-parentBean" abstract="true" parent="AttributeDefinition">
	    	<property name="name" value="positionName"/>
	    	<property name="label" value="Position Name"/>
	    	<property name="shortLabel" value="Position Name"/>
	    	<property name="maxLength" value="128"/>
	    	<property name="control">
	    	  	<bean parent="TextControlDefinition" p:size="12"/>
	    	</property>
    </bean>
            
    <bean id="KualiDeveloper-employeeId" parent="KualiDeveloper-employeeId-parentBean"/>
    <bean id="KualiDeveloper-employeeId-parentBean" abstract="true" parent="AttributeDefinition">
	    	<property name="name" value="employeeId"/>
	    	<property name="label" value="Employee ID"/>
	    	<property name="shortLabel" value="Emp ID"/>
	    	<property name="maxLength" value="16"/>
	    	<property name="control">
	    	  	<bean parent="TextControlDefinition" p:size="8"/>
	    	</property>
    </bean>
      
  	<bean id="KualiDeveloper-socialSecurityNumber" parent="KualiDeveloper-socialSecurityNumber-parentBean" />
    <bean id="KualiDeveloper-socialSecurityNumber-parentBean" abstract="true" parent="AttributeDefinition">
        	<property name="name" value="socialSecurityNumber"/>
        	<property name="label" value="Social Security Number"/>
        	<property name="shortLabel" value="SSN"/>
        	<property name="maxLength" value="12"/>
        	<property name="control">
        		<bean parent="TextControlDefinition" p:size="12"/>
        	</property>
        	<property name="validationPattern">
      		<bean parent="AlphaNumericValidationPattern">
      			<property name="maxLength" value="12"/>
      		</bean>
        	</property>
        <property name="attributeSecurity">
            <bean parent="AttributeSecurity">
                <property name="mask" value="true" /> 
                <property name="maskFormatter">
                    <bean parent="MaskFormatterLiteral" p:literal="*****************"/>
                </property>
                <property name="partialMask" value="true"/> 
                <property name="partialMaskFormatter">
                    <bean parent="MaskFormatterSubString" p:maskLength="5"/> 
                </property>
            </bean>
        </property>    
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.addressTypeCode"
          parent="KualiDeveloper-kualiAddresses.addressTypeCode-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.addressTypeCode-parentBean" abstract="true"
          parent="KualiAddress-addressTypeCode">
        <property name="name" value="kualiAddresses.addressTypeCode"/>
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.streetAddress"
          parent="KualiDeveloper-kualiAddresses.streetAddress-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.streetAddress-parentBean" abstract="true"
          parent="KualiAddress-streetAddress">
        <property name="name" value="kualiAddresses.streetAddress"/>
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.streetAddressExtra"
          parent="KualiDeveloper-kualiAddresses.streetAddressExtra-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.streetAddressExtra-parentBean" abstract="true"
          parent="KualiAddress-streetAddressExtra">
        <property name="name" value="kualiAddresses.streetAddressExtra"/>
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.city"
          parent="KualiDeveloper-kualiAddresses.city-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.city-parentBean" abstract="true"
          parent="KualiAddress-city">
        <property name="name" value="kualiAddresses.city"/>
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.stateCode"
          parent="KualiDeveloper-kualiAddresses.stateCode-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.stateCode-parentBean" abstract="true"
          parent="KualiAddress-stateCode">
        <property name="name" value="kualiAddresses.stateCode"/>
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.zipCode"
          parent="KualiDeveloper-kualiAddresses.zipCode-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.zipCode-parentBean" abstract="true"
          parent="KualiAddress-zipCode">
        <property name="name" value="kualiAddresses.zipCode"/>
    </bean>
    
    <bean id="KualiDeveloper-kualiAddresses.countryCode"
          parent="KualiDeveloper-kualiAddresses.countryCode-parentBean"/>
    <bean id="KualiDeveloper-kualiAddresses.countryCode-parentBean" abstract="true"
          parent="KualiAddress-countryCode">
        <property name="name" value="kualiAddresses.countryCode"/>
    </bean>
   
    <bean id="KualiDeveloper-active" parent="KualiDeveloper-active-parentBean"/>
    <bean id="KualiDeveloper-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
        <property name="name" value="active"/>
    </bean>  
	
    <!-- Business Object Inquiry Definition -->  
    <bean id="KualiDeveloper-inquiryDefinition" parent="KualiDeveloper-inquiryDefinition-parentBean"/>
    <bean id="KualiDeveloper-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Kuali Developer Inquiry"/>
        <property name="inquirySections">
            <list>
                <ref bean="KualiDeveloper-inquirySectionDefinition-generalInformation" /> 
                <ref bean="KualiDeveloper-inquirySectionDefinition-address"/>
            </list>  
        </property>
    </bean>
  
    <bean id="KualiDeveloper-inquirySectionDefinition-generalInformation" parent="KualiDeveloper-inquirySectionDefinition-generalInformation-parentBean"/>
    <bean id="KualiDeveloper-inquirySectionDefinition-generalInformation-parentBean" parent="InquirySectionDefinition" abstract="true">
        <property name="id" value="generalInformationSection"/>
        <property name="title" value="General Information"/>
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="employeeId"/>
                <bean parent="FieldDefinition" p:attributeName="firstName"/>
                <bean parent="FieldDefinition" p:attributeName="lastName"/>
                <bean parent="FieldDefinition" p:attributeName="positionName"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>
    
    <bean id="KualiDeveloper-inquirySectionDefinition-address" parent="KualiDeveloper-inquirySectionDefinition-address-parentBean"/>
    <bean id="KualiDeveloper-inquirySectionDefinition-address-parentBean" abstract="true" parent="InquirySectionDefinition">
        <property name="id" value="addressSection"/>
        <property name="title" value="Address"/>
        <property name="inquiryFields">
            <list>
                <ref bean="KualiDeveloper-inquiryCollectionDefinition-kualiAddresses"/>
            </list>
        </property>
    </bean>
    
    <bean id="KualiDeveloper-inquiryCollectionDefinition-kualiAddresses"
          parent="KualiDeveloper-inquiryCollectionDefinition-kualiAddresses-parentBean"/>
    <bean id="KualiDeveloper-inquiryCollectionDefinition-kualiAddresses-parentBean" abstract="true"
          parent="InquiryCollectionDefinition">
        <property name="attributeName" value="kualiAddresses"/>
        <property name="businessObjectClass" value="edu.cornell.kfs.sys.businessobject.KualiAddress"/>
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="addressId"/>
                <bean parent="FieldDefinition" p:attributeName="addressTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="streetAddress"/>
                <bean parent="FieldDefinition" p:attributeName="streetAddressExtra"/>
                <bean parent="FieldDefinition" p:attributeName="city"/>
                <bean parent="FieldDefinition" p:attributeName="stateCode"/>
                <bean parent="FieldDefinition" p:attributeName="zipCode"/>
                <bean parent="FieldDefinition" p:attributeName="countryCode"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
        <property name="summaryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="addressTypeCode"/>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="KualiDeveloper-lookupDefinition" parent="KualiDeveloper-lookupDefinition-parentBean"/>
    <bean id="KualiDeveloper-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="KualiDeveloper Lookup"/>
        <property name="defaultSort">
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>firstName</value> 
                        <value>lastName</value>
                        <value>employeeId</value>  
                        <value>positionName</value>     
                        <value>active</value>       
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="firstName"/>
                <bean parent="FieldDefinition" p:attributeName="lastName"/>
                <bean parent="FieldDefinition" p:attributeName="employeeId"/>
                <bean parent="FieldDefinition" p:attributeName="positionName"/>
                <bean parent="FieldDefinition" p:defaultValue="Y" p:attributeName="active"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="firstName"/>
                <bean parent="FieldDefinition" p:attributeName="lastName"/>
                <bean parent="FieldDefinition" p:attributeName="employeeId"/>
                <bean parent="FieldDefinition" p:attributeName="positionName"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>
    
</beans>