<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="cu-batchFileDirectoryPathValuesFinder"
          class="edu.cornell.kfs.sys.businessobject.options.CuBatchFileDirectoryPathValuesFinder"/>

    <bean id="BatchFile-path" parent="BatchFile-path-parentBean">
        <property name="control">
            <bean parent="MultiselectControlDefinition" p:hierarchical="true" p:size="15" p:valuesFinder-ref="cu-batchFileDirectoryPathValuesFinder"/>
        </property>
    </bean>

</beans>
