<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">       

    <bean id="KualiAddressType" parent="KualiAddressType-parentBean"/>
    <bean id="KualiAddressType-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="edu.cornell.kfs.sys.businessobject.KualiAddressType"/>
        <property name="inquiryDefinition">
            <ref bean="KualiAddressType-inquiryDefinition"/>
        </property>
        <property name="lookupDefinition">
            <ref bean="KualiAddressType-lookupDefinition"/>
        </property>
        <property name="titleAttribute" value="addressTypeCode"/>
        <property name="objectLabel" value="Kuali Address Type"/>
        <property name="attributes">
            <list>
                <ref bean="KualiAddressType-active"/>
                <ref bean="KualiAddressType-addressTypeCode"/>
                <ref bean="KualiAddressType-addressTypeName"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="KualiAddressType-active" parent="KualiAddressType-active-parentBean"/>
    <bean id="KualiAddressType-active-parentBean" abstract="true"
          parent="GenericAttributes-activeIndicator">
        <property name="name" value="active"/>
    </bean>
    
    <bean id="KualiAddressType-addressTypeCode" parent="KualiAddressType-addressTypeCode-parentBean"/>
    <bean id="KualiAddressType-addressTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="addressTypeCode"/>
        <property name="forceUppercase" value="true"/>
        <property name="label" value="Address Type Code"/>
        <property name="shortLabel" value="Code"/>
        <property name="maxLength" value="1"/>
        <property name="validationPattern">
            <bean parent="AlphaValidationPattern" />
        </property>
        <property name="required" value="true"/>
        <property name="control">
            <ref bean="TwoCharacterTextControl"/>
        </property>
    </bean>
    
    <bean id="KualiAddressType-addressTypeName" parent="KualiAddressType-addressTypeName-parentBean"/>
    <bean id="KualiAddressType-addressTypeName-parentBean" abstract="true"
          parent="AttributeDefinition">
        <property name="name" value="addressTypeName"/>
        <property name="label" value="Address Type Name"/>
        <property name="shortLabel" value="Name"/>
        <property name="maxLength" value="40"/>
        <property name="validationPattern">
            <ref bean="AnyCharacterWithWhitespaceValidation"/>
        </property>
        <property name="required" value="true"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="50"/>
        </property>
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="KualiAddressType-inquiryDefinition" parent="KualiAddressType-inquiryDefinition-parentBean"/>
    <bean id="KualiAddressType-inquiryDefinition-parentBean" abstract="true"
          parent="InquiryDefinition">
        <property name="title" value="Address Type Inquiry"/>
        <property name="inquirySections">
            <list>
                <ref bean="KualiAddressType-inquirySectionDefinition"/>
            </list>
        </property>
    </bean>

    <bean id="KualiAddressType-inquirySectionDefinition" parent="KualiAddressType-inquirySectionDefinition-parentBean"/>
    <bean id="KualiAddressType-inquirySectionDefinition-parentBean" parent="InquirySectionDefinition" abstract="true">
        <property name="title" value=""/>
        <property name="numberOfColumns" value="1"/>
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="addressTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="addressTypeName"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="KualiAddressType-lookupDefinition" parent="KualiAddressType-lookupDefinition-parentBean"/>
    <bean id="KualiAddressType-lookupDefinition-parentBean" abstract="true"
          parent="LookupDefinition">
        <property name="title" value="Address Type Lookup"/>
        <property name="lookupableID" value="addressTypeLookupable"/> 

        <property name="defaultSort">
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>addressTypeCode</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="addressTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="addressTypeName"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="addressTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="addressTypeName"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>

</beans>