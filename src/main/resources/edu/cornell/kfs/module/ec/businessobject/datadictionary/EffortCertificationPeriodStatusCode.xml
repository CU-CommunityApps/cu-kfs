<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="EffortCertificationPeriodStatusCode" parent="EffortCertificationPeriodStatusCode-parentBean"/>
    <bean id="EffortCertificationPeriodStatusCode-parentBean" abstract="true" parent="BusinessObjectEntry"
          p:businessObjectClass="org.kuali.kfs.module.ec.businessobject.EffortCertificationPeriodStatusCode"
          p:objectLabel="Report Period Status Code" p:name="EffortCertificationPeriodStatusCode"
          p:businessObjectAdminService-ref="defaultBoAdminService"
          p:inquiryDefinition-ref="EffortCertificationPeriodStatusCode-inquiryDefinition"
          p:lookupDefinition-ref="EffortCertificationPeriodStatusCode-lookupDefinition"
          p:titleAttribute="effortCertificationReportPeriodStatusCode">
        <property name="attributes">
            <list>
                <ref bean="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusCode"/>
                <ref bean="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusDescription"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusCode"
          parent="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusCode-parentBean"/>
    <bean id="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusCode-parentBean"
          abstract="true" parent="AttributeDefinition" p:name="effortCertificationReportPeriodStatusCode"
          p:forceUppercase="true" p:label="Report Period Status Code" p:shortLabel="Code" p:maxLength="1"
          p:validationPattern-ref="AnyCharacterValidation">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="3"/>
        </property>
    </bean>

    <bean id="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusDescription"
          parent="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusDescription-parentBean"/>
    <bean id="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusDescription-parentBean"
          abstract="true" parent="AttributeDefinition" p:name="effortCertificationReportPeriodStatusDescription"
          p:forceUppercase="false" p:label="Report Period Status Description" p:shortLabel="Description"
          p:maxLength="40" p:validationPattern-ref="AnyCharacterWithWhitespaceValidation">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="42"/>
        </property>
    </bean>

    <!-- Business Object Inquiry Definition -->

    <bean id="EffortCertificationPeriodStatusCode-inquiryDefinition"
          parent="EffortCertificationPeriodStatusCode-inquiryDefinition-parentBean"/>
    <bean id="EffortCertificationPeriodStatusCode-inquiryDefinition-parentBean" abstract="true"
          parent="InquiryDefinition" p:title="Report Period Status Code Inquiry">
        <property name="inquirySections">
            <list>
                <ref bean="EffortCertificationPeriodStatusCode-inquirySectionDefinition"/>
            </list>
        </property>
    </bean>

    <bean id="EffortCertificationPeriodStatusCode-inquirySectionDefinition"
          parent="EffortCertificationPeriodStatusCode-inquirySectionDefinition-parentBean"/>
    <bean id="EffortCertificationPeriodStatusCode-inquirySectionDefinition-parentBean"
          parent="InquirySectionDefinition" abstract="true" p:title="" p:numberOfColumns="1">
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="effortCertificationReportPeriodStatusCode"/>
                <bean parent="FieldDefinition" p:attributeName="effortCertificationReportPeriodStatusDescription"/>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->

    <bean id="EffortCertificationPeriodStatusCode-lookupDefinition"
          parent="EffortCertificationPeriodStatusCode-lookupDefinition-parentBean"/>
    <bean id="EffortCertificationPeriodStatusCode-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
          p:title="Report Period Status Code Lookup" p:lookupSearchService-ref="defaultLookupSearchService">
        <property name="lookupAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusCode"/>
            </list>
        </property>
        <property name="lookupResultAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusCode"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="EffortCertificationPeriodStatusCode-effortCertificationReportPeriodStatusDescription"/>
            </list>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="effortCertificationReportPeriodStatusCode"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="effortCertificationReportPeriodStatusCode"/>
                <bean parent="FieldDefinition" p:attributeName="effortCertificationReportPeriodStatusDescription"/>
            </list>
        </property>
    </bean>
</beans>
