<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">
  
    <!-- Attribute Definitions -->  
    <bean id="Customer-customerContactName" parent="Customer-customerContactName-parentBean">
        <property name="maxLength" value="50"/>
    </bean>

    <bean id="Customer-customerContactPhoneNumber" parent="Customer-customerContactPhoneNumber-parentBean">
        <property name="label" value="Customer Phone Number"/>
    </bean>
    
    <bean id="Customer-customerEmailAddress" parent="Customer-customerEmailAddress-parentBean">
        <property name="label" value="Customer Email Address" />
        <property name="forceUppercase" value="false"/>
    </bean> 

    <!-- Business Object Lookup Definition -->
    <bean parent="DataDictionaryBeanOverride">
        <property name="beanName" value="Customer-lookupDefinition" />
        <property name="fieldOverrides">
            <list>
                <bean parent="FieldOverrideForListElementInsert">
                   <property name="propertyName" value="resultFields" />
                   <property name="propertyNameForElementCompare" value="attributeName" />
                   <property name="element">
                       <bean parent="FieldDefinition" p:attributeName="customerTaxNbr" />
                   </property>
                   <property name="insertAfter">
                       <list>
                           <bean parent="FieldDefinition" p:attributeName="customerEmailAddress" />
                       </list>
                   </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>