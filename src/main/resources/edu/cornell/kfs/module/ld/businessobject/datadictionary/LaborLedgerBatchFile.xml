    <import resource="classpath:edu/cornell/kfs/module/ld/cu-ld-lookup-beans.xml"/>

    <import resource="classpath:org/kuali/kfs/kns/datadictionary/kns-lookup-beans.xml"/>

    <bean id="LaborLedgerBatchFile" parent="LaborLedgerBatchFile-parentBean"/>
    <bean id="LaborLedgerBatchFile-parentBean" abstract="true" parent="BusinessObjectEntry"
          p:actionsProvider-ref="batchFileActionsProvider"
          p:businessObjectAdminService-ref="batchFileAdminService"
          p:businessObjectClass="edu.cornell.kfs.module.ld.businessobject.LaborLedgerBatchFile"
          p:lookupDefinition-ref="LaborLedgerBatchFile-lookupDefinition"
          p:name="LaborLedgerBatchFile"
          p:objectLabel="Labor Ledger Batch File"
          p:searchService-ref="laborLedgerFileLookupSearchService"
          p:titleAttribute="name">
        <property name="attributes">
            <list>
                <ref bean="LaborLedgerBatchFile-path"/>
                <ref bean="LaborLedgerBatchFile-fileName"/>
                <ref bean="LaborLedgerBatchFile-lastModifiedDate"/>
                <ref bean="LaborLedgerBatchFile-fileSize"/>
            </list>
        </property>
    </bean>

    <bean id="batchFileDirectoryPathValuesFinder"
          class="org.kuali.kfs.sys.businessobject.options.BatchFileDirectoryPathValuesFinder"/>

    <!-- Attribute Definitions -->

    <bean id="LaborLedgerBatchFile-path" parent="LaborLedgerBatchFile-path-parentBean"/>
    <bean id="LaborLedgerBatchFile-path-parentBean" abstract="true" parent="AttributeDefinition" p:name="path"
          p:forceUppercase="false" p:label="File Path" p:shortLabel="Path" p:type="OTHER">
    <property name="control">
      <bean parent="TextControlDefinition"/>
    </property>
    <property name="attributeSecurity">
       <bean parent="AttributeSecurity">
          <property name="readOnly" value="true"/>
      </bean>
   </property>
    </bean>

    <bean id="batchFileDirectoryPathHierarchicalControlValuesFinder"
          class="org.kuali.kfs.sys.businessobject.options.BatchFileDirectoryPathHierarchicalControlValuesFinder"/>

    <bean id="LaborLedgerBatchFile-fileName" parent="LaborLedgerBatchFile-fileName-parentBean"/>
    <bean id="LaborLedgerBatchFile-fileName-parentBean" abstract="true" parent="AttributeDefinition" p:name="fileName"
          p:forceUppercase="false" p:label="File Name" p:shortLabel="File Name" p:maxLength="40">
        <property name="control">
            <bean parent="TextControlDefinition"/>
        </property>
    </bean>

    <bean id="LaborLedgerBatchFile-lastModifiedDate" parent="LaborLedgerBatchFile-lastModifiedDate-parentBean"/>
    <bean id="LaborLedgerBatchFile-lastModifiedDate-parentBean" abstract="true" parent="GenericAttributes-genericTimestamp"
          p:name="lastModifiedDate" p:label="Last Modified Date" p:shortLabel="Last Modified Date"
          p:formatterClass="org.kuali.kfs.core.web.format.TimestampAMPMFormatter" p:type="DATE_RANGE"/>

    <bean id="LaborLedgerBatchFile-fileSize" parent="LaborLedgerBatchFile-fileSize-parentBean"/>
    <bean id="LaborLedgerBatchFile-fileSize-parentBean" abstract="true" parent="AttributeDefinition" p:name="fileSize"
          p:forceUppercase="false" p:label="File Size" p:shortLabel="File Size" p:maxLength="40"
          p:control-ref="StandardNameTextControl" p:type="INTEGER"/>

    <!-- Business Object Lookup Definition -->

    <bean id="LaborLedgerBatchFile-lookupDefinition" parent="LaborLedgerBatchFile-lookupDefinition-parentBean"/>
    <bean id="LaborLedgerBatchFile-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
          p:title="Labor Ledger Batch File Lookup">
        <property name="formAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.datadictionary.legacy.model.FormAttributeDefinition" parent="LaborLedgerBatchFile-path"/>
                <bean class="org.kuali.kfs.datadictionary.legacy.model.FormAttributeDefinition"
                      parent="LaborLedgerBatchFile-fileName"/>
                <bean class="org.kuali.kfs.datadictionary.legacy.model.FormAttributeDefinition"
                      parent="LaborLedgerBatchFile-lastModifiedDate"/>
            </list>
        </property>
        <property name="displayAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.datadictionary.legacy.model.DisplayAttributeDefinition"
                      parent="LaborLedgerBatchFile-path"/>
                <bean class="org.kuali.kfs.datadictionary.legacy.model.DisplayAttributeDefinition"
                      parent="LaborLedgerBatchFile-fileName"/>
                <bean class="org.kuali.kfs.datadictionary.legacy.model.DisplayAttributeDefinition"
                      parent="LaborLedgerBatchFile-lastModifiedDate" p:type="DATE_TIME" />
                <bean class="org.kuali.kfs.datadictionary.legacy.model.DisplayAttributeDefinition"
                      parent="LaborLedgerBatchFile-fileSize"/>
            </list>
        </property>
    </bean>
    
    <bean id="laborLedgerFileLookupSearchService"
          class="edu.cornell.kfs.module.ld.businessobject.lookup.LaborLedgerBatchFileLookupableHelperServiceImpl"
          parent="searchService"/>

    <bean id="batchFileAdminService" class="org.kuali.kfs.sys.batch.service.BatchFileAdminService"/>
    <bean id="batchFileActionsProvider"
          class="org.kuali.kfs.sys.businessobject.actions.BatchFileActionsProvider"/>
</beans>