<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="CapitalAssetSystemType" parent="CapitalAssetSystemType-parentBean"/>
    <bean id="CapitalAssetSystemType-parentBean" abstract="true" parent="BusinessObjectEntry"
          p:objectLabel="Capital Asset System Type"
          p:businessObjectAdminService-ref="defaultBoAdminService" p:name="CapitalAssetSystemType"
          p:businessObjectClass="org.kuali.kfs.module.purap.businessobject.CapitalAssetSystemType"
          p:inquiryDefinition-ref="CapitalAssetSystemType-inquiryDefinition"
          p:lookupDefinition-ref="CapitalAssetSystemType-lookupDefinition"
          p:titleAttribute="capitalAssetSystemTypeCode">
        <property name="attributes">
            <list>
                <ref bean="CapitalAssetSystemType-capitalAssetSystemTypeCode"/>
                <ref bean="CapitalAssetSystemType-capitalAssetSystemTypeDescription"/>
                <ref bean="CapitalAssetSystemType-allowedCapitalAssetSystemStateCodes"/>
                <ref bean="CapitalAssetSystemType-active"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="CapitalAssetSystemType-capitalAssetSystemTypeCode"
          parent="CapitalAssetSystemType-capitalAssetSystemTypeCode-parentBean"/>
    <bean id="CapitalAssetSystemType-capitalAssetSystemTypeCode-parentBean" abstract="true"
          parent="AttributeDefinition" p:name="capitalAssetSystemTypeCode" p:forceUppercase="true"
          p:label="Capital Asset System Type Code" p:shortLabel="Capital Asset System Type Code" p:maxLength="4"
          p:validationPattern-ref="AnyCharacterValidation">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="47"/>
        </property>
    </bean>

    <bean id="CapitalAssetSystemType-capitalAssetSystemTypeDescription"
          parent="CapitalAssetSystemType-capitalAssetSystemTypeDescription-parentBean"/>
    <bean id="CapitalAssetSystemType-capitalAssetSystemTypeDescription-parentBean" abstract="true"
          parent="AttributeDefinition" p:name="capitalAssetSystemTypeDescription"
          p:label="Capital Asset System Description" p:shortLabel="Description" p:maxLength="45"
          p:validationPattern-ref="AnyCharacterWithWhitespaceValidation">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="47"/>
        </property>
    </bean>

    <bean id="CapitalAssetSystemType-allowedCapitalAssetSystemStateCodes"
          parent="CapitalAssetSystemType-allowedCapitalAssetSystemStateCodes-parentBean"/>
    <bean id="CapitalAssetSystemType-allowedCapitalAssetSystemStateCodes-parentBean" abstract="true"
          parent="AttributeDefinition" p:name="allowedCapitalAssetSystemStateCodes"
          p:label="Allowed Capital Asset System State Codes" p:shortLabel="Allowed State Cds" p:maxLength="50"
          p:validationPattern-ref="AnyCharacterWithWhitespaceValidation">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="50"/>
        </property>
    </bean>

    <bean id="CapitalAssetSystemType-active" parent="CapitalAssetSystemType-active-parentBean"/>
    <bean id="CapitalAssetSystemType-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator"
          p:name="active"/>

    <!-- Business Object Inquiry Definition -->

    <bean id="CapitalAssetSystemType-inquiryDefinition" parent="CapitalAssetSystemType-inquiryDefinition-parentBean"/>
    <bean id="CapitalAssetSystemType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition"
          p:title="Capital Asset System Type Inquiry">
        <property name="inquirySections">
            <list>
                <ref bean="CapitalAssetSystemType-inquirySectionDefinition"/>
            </list>
        </property>
    </bean>

    <bean id="CapitalAssetSystemType-inquirySectionDefinition"
          parent="CapitalAssetSystemType-inquirySectionDefinition-parentBean"/>
    <bean id="CapitalAssetSystemType-inquirySectionDefinition-parentBean" parent="InquirySectionDefinition"
          abstract="true" p:title="" p:numberOfColumns="1">
        <property name="inquiryFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="capitalAssetSystemTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="capitalAssetSystemTypeDescription"/>
                <bean parent="FieldDefinition" p:attributeName="allowedCapitalAssetSystemStateCodes"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->

    <bean id="CapitalAssetSystemType-lookupDefinition" parent="CapitalAssetSystemType-lookupDefinition-parentBean"/>
    <bean id="CapitalAssetSystemType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition"
          p:title="Capital Asset System Type Lookup" p:lookupSearchService-ref="defaultLookupSearchService">
        <property name="lookupAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="CapitalAssetSystemType-capitalAssetSystemTypeCode"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="CapitalAssetSystemType-capitalAssetSystemTypeDescription"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupAttributeDefinition"
                      parent="CapitalAssetSystemType-active"
                      p:control-ref="GenericAttributes-genericBooleanYNBoth-lookupControl" p:defaultValue="Y"/>
            </list>
        </property>
        <property name="lookupResultAttributeDefinitions">
            <list>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="CapitalAssetSystemType-capitalAssetSystemTypeCode"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="CapitalAssetSystemType-capitalAssetSystemTypeDescription"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="CapitalAssetSystemType-allowedCapitalAssetSystemStateCodes"/>
                <bean class="org.kuali.kfs.krad.datadictionary.LookupResultAttributeDefinition"
                      parent="CapitalAssetSystemType-active"/>
            </list>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="capitalAssetSystemTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="capitalAssetSystemTypeDescription"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="capitalAssetSystemTypeCode"/>
                <bean parent="FieldDefinition" p:attributeName="capitalAssetSystemTypeDescription"/>
                <bean parent="FieldDefinition" p:attributeName="allowedCapitalAssetSystemStateCodes"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
        </property>
    </bean>
</beans>
