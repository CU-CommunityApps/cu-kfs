<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="IWantNoteMaintenanceDocument" parent="IWantNoteMaintenanceDocument-parentBean"/>
    <bean id="IWantNoteMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry"
          p:businessObjectClass="edu.cornell.kfs.module.purap.businessobject.IWantNote"
          p:maintainableClass="org.kuali.kfs.sys.document.FinancialSystemMaintainable" p:documentTypeName="INTE"
          p:documentAuthorizerClass="org.kuali.kfs.kns.document.authorization.MaintenanceDocumentAuthorizerBase"
          p:workflowAttributes-ref="IWantNoteMaintenanceDocument-workflowAttributes">
        <property name="maintainableSections">
            <list>
                <ref bean="IWantNoteMaintenanceDocument-EditIWantNote"/>
            </list>
        </property>
        <property name="lockingKeys">
            <list>
                <value>noteIdentifier</value>
            </list>
        </property>
    </bean>

    <!-- Maintenance Section Definitions -->

    <bean id="IWantNoteMaintenanceDocument-EditIWantNote"
          parent="IWantNoteMaintenanceDocument-EditIWantNote-parentBean"/>
    <bean id="IWantNoteMaintenanceDocument-EditIWantNote-parentBean" abstract="true"
          parent="MaintainableSectionDefinition" p:id="Edit IWantNote" p:title="Edit IWant Note">
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="noteIdentifier" p:required="true" p:unconditionallyReadOnly="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="noteText" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:defaultValue="true" p:name="active"/>
            </list>
        </property>
    </bean>

    <!-- workflow attributes -->
    <alias alias="IWantNoteMaintenanceDocument-workflowAttributes" name="EmptyWorkflowAttributes"/>
</beans>
