    <property name="validationMap" ref="IWantDocument-validations"/>
	<property name="relationships">
    	<list>
    		<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="vendorDetail"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="vendorNumber" p:targetName="vendorNumber"/>
                </list>
              </property>
    		</bean>
    		<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="collegeChartForSearch"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="collegeLevelChartForSearch" p:targetName="chartOfAccountsCode"/>
                </list>
              </property>
    		</bean>
    		<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="collegeOrgForSearch"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="collegeLevelOrgCodeForSearch" p:targetName="organizationCode"/>
                </list>
              </property>
    		</bean>
    		<bean parent="RelationshipDefinition">
              <property name="objectAttributeName" value="departmentOrgForSearch"/>
              <property name="primitiveAttributes">
                <list>
                  <bean parent="PrimitiveAttributeDefinition" p:sourceName="departmentLevelOrgCodeForSearch" p:targetName="organizationCode"/>
                </list>
              </property>
    		</bean>
		</list>
	</property>
	<property name="accountingLineGroups">
    	<map>
    		<entry key="source" value-ref="IWantDocument-sourceAccountingLineGroup"/>
    	</map>
    </property>

	<property name="webScriptFiles">
		<list>
			<value>dwr/interface/AccountService.js</value>
			<value>dwr/interface/CommodityCodeService.js</value>
			<value>dwr/interface/ItemUnitOfMeasureService.js</value>
			<value>dwr/interface/IWantAmountUtil.js</value>
			<value>dwr/interface/IWantDocumentService.js</value>
			<value>dwr/interface/PersonService.js</value>
			<value>scripts/module/purap/iWantDoc.js</value>
			<value>scripts/module/purap/objectInfo.js</value>
			<value>scripts/vnd/objectInfo.js</value>
		</list>
	</property>

