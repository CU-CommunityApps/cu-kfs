  <bean id="sciQuestDataAccess" parent="platformAwareDao"  
          class="org.kuali.kfs.module.purap.dataaccess.impl.SciQuestDataAccessImpl" />
    
    <bean id="sciQuestService" parent="sciQuestService-parent"/>    
    <bean id="sciQuestService-parent"
          class="org.kuali.kfs.module.purap.document.service.impl.SciQuestServiceImpl">
       <property name="sciquestDataAccess" ref="sciQuestDataAccess" />
       <property name="businessObjectService" ref="businessObjectService" />
    </bean> 

	<bean id="electronicInvoiceExtractJob" parent="scheduledJobDescriptor">
		<property name="steps">
			<list>
				<ref bean="electronicInvoiceExtractStep" />
				<ref bean="electronicInvoiceRouteStep" />
			</list>
		</property>
	</bean>
	
	<bean id="electronicInvoiceRouteStep" class="org.kuali.kfs.module.purap.batch.ElectronicInvoiceRouteStep" parent="step">
		<property name="electronicInvoiceHelperService" ref="electronicInvoiceHelperService" />
	</bean>
	
	<bean id="collegeLevelOrganizationDao" parent="platformAwareDaoJdbc" class="edu.cornell.kfs.module.purap.dataaccess.impl.LevelOrganizationDaoJdbc">
	
