<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
    <documentTypes xmlns="ns:workflow/DocumentType" xsi:schemaLocation="ns:workflow/DocumentType resource:DocumentType">
        <documentType>
            <name>GLCP</name>
            <parent>GL</parent>
            <label>General Ledger Correction Process</label>
            <authorizer>org.kuali.kfs.krad.workflow.authorizer.CfDocumentTypeAuthorizer</authorizer>
            <docHandler>${application.url}/generalLedgerGeneralLedgerCorrectionProcess.do?methodToCall=docHandler</docHandler>
            <helpDefinitionURL>default.htm?turl=WordDocuments%2Fgeneralledgercorrectionprocess.htm</helpDefinitionURL>
            <docSearchHelpURL>default.htm?turl=WordDocuments%2Fgeneralledgercorrectionprocess.htm</docSearchHelpURL>
            <active>true</active>
            <routingVersion>2</routingVersion>
            <routePaths>
                <routePath>
                    <start name="AdHoc" nextNode="Management"/>
                    <role name="Management" nextNode="autoApproveForAsynchronousProcessing"/>
                    <simple name="autoApproveForAsynchronousProcessing"/>
                </routePath>
            </routePaths>
            <routeNodes>
                <start name="AdHoc"/>
                <role name="Management">
                    <qualifierResolverClass>
                        org.kuali.kfs.krad.workflow.attribute.DataDictionaryQualifierResolver
                    </qualifierResolverClass>
                    <activationType>P</activationType>
                </role>
                <simple name="autoApproveForAsynchronousProcessing">
                    <type>org.kuali.kfs.kew.engine.node.NoOpNode</type>
                </simple>
            </routeNodes>
        </documentType>
    </documentTypes>
</data>
