	<bean id="concurRequestExtractInputFileType" parent="FlatFileParser">
        <property name="flatFileSpecification">
            <bean parent="DelimitedFlatFileSpecification" p:delimiter="|" p:defaultBusinessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurRequestExtractFile">
                <property name="objectSpecifications">
                    <list>
                        <bean parent="FlatFileObjectSpecification"
                              p:linePrefix="EXTRACT"
                              p:businessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurRequestExtractFile">
                            <property name="parseProperties">
                                <list>
                                    <bean parent="DelimitedFlatFilePropertySpecification"
                                          p:lineSegmentIndex="1"
                                          p:formatterClass="org.kuali.kfs.sys.businessobject.format.BatchDateFormatter"
                                          p:dateFormat="yyyy-MM-dd"
                                          p:propertyName="batchDate"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification"
                                          p:lineSegmentIndex="2"
                                          p:formatterClass="org.kuali.rice.core.web.format.IntegerFormatter"
                                          p:propertyName="recordCount"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification"
                                          p:lineSegmentIndex="3"
                                          p:formatterClass="org.kuali.kfs.sys.businessobject.format.ExplicitKualiDecimalFormatter"
                                          p:propertyName="totalApprovedAmount"/>
                                </list>
                            </property>
                        </bean>
                        <bean parent="FlatFileObjectSpecification"
                              p:linePrefix="REQUEST DETAIL"
                              p:businessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurRequestExtractRequestDetailFileLine"
                              p:parentBusinessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurRequestExtractFile"
                              p:parentTargetProperty="requestDetails">
                            <property name="parseProperties">
                                <list>
