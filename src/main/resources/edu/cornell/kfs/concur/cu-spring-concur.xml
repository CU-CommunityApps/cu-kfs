	<bean id="standardAccountExtractFileType" parent="FlatFileParser">
        <property name="flatFileSpecification">
            <bean class="org.kuali.kfs.sys.batch.DelimitedFlatFileSpecification">
                <property name="delimiter" value="|"/>
                <property name="objectSpecifications">
                    <list>
                        <bean parent="FlatFilePrefixObjectSpecification"
                              p:linePrefix="EXTRACT" p:businessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurStandardAccountingExtractFile">
                            <property name="parseProperties">
                                <list>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="1" p:propertyName="batchDate"
                                    	p:formatterClass="org.kuali.kfs.sys.businessobject.format.BatchDateFormatter"
                                    	p:dateFormat="yyyy-MM-dd"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="2" p:propertyName="recordCount"
                                    	p:formatterClass="org.kuali.rice.core.web.format.IntegerFormatter"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="3" p:propertyName="journalAmountTotal"
                                    	p:formatterClass="edu.cornell.kfs.sys.businessobject.format.RemovePlusSignKualiDecimalFormatter"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="4" p:propertyName="batchId"/>
                                </list>
                            </property>
                        </bean>
                        <bean parent="FlatFilePrefixObjectSpecification"
                              p:linePrefix="DETAIL"
                              p:businessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurStandardAccountingExtractDetailLine"
                              p:parentBusinessObjectClass="edu.cornell.kfs.concur.batch.businessobject.ConcurStandardAccountingExtractFile"
                              p:parentTargetProperty="concurStandardAccountingExtractDetailLines">
                            <property name="parseProperties">
                                <list>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="1" p:propertyName="batchID"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="2" p:propertyName="batchDate"
                                    	p:formatterClass="org.kuali.kfs.sys.businessobject.format.BatchDateFormatter"
                                    	p:dateFormat="yyyy-MM-dd"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="3" p:propertyName="sequenceNumber"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="4" p:propertyName="employeeId"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="5" p:propertyName="employeeLastName"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="6" p:propertyName="employeeFirstName"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="7" p:propertyName="employeeMiddleInitital"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="8" p:propertyName="employeeGroupId"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="18" p:propertyName="reportId"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="32" p:propertyName="policy"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="34" p:propertyName="reportChartOfAccountsCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="35" p:propertyName="reportAccountNumber"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="36" p:propertyName="reportSubAccountNumber"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="37" p:propertyName="reportSubObjectCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="38" p:propertyName="reportProjectCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="39" p:propertyName="reportOrgRefId"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="40" p:propertyName="employeeStatus"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="60" p:propertyName="reportEntryId"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="62" p:propertyName="expenseType"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="67" p:propertyName="reportEntryIsPersonalFlag"
                                        p:formatterClass="org.kuali.rice.core.web.format.BooleanFormatter"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="125" p:propertyName="paymentCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="166" p:propertyName="journalAccountCode"
                                    	p:formatterClass="edu.cornell.kfs.concur.batch.businessobject.format.ConcurJournalAccountCodeFormatter"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="166" p:propertyName="journalAccountCodeOverridden"
                                    	p:formatterClass="edu.cornell.kfs.concur.batch.businessobject.format.ConcurJournalAccountCodeOverriddenFormatter"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="167" p:propertyName="jounalDebitCredit"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="168" p:propertyName="journalAmount"
                                    	p:formatterClass="edu.cornell.kfs.sys.businessobject.format.RemovePlusSignKualiDecimalFormatter"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="168" p:propertyName="journalAmountString"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="186" p:propertyName="cashAdvanceKey"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="190" p:propertyName="chartOfAccountsCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="191" p:propertyName="accountNumber"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="192" p:propertyName="subAccountNumber"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="193" p:propertyName="subObjectCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="194" p:propertyName="projectCode"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="195" p:propertyName="orgRefId"/>
                                    <bean parent="DelimitedFlatFilePropertySpecification" p:lineSegmentIndex="324" p:propertyName="reportEndDate"
                                            p:formatterClass="org.kuali.kfs.sys.businessobject.format.BatchDateFormatter"
                                            p:dateFormat="yyyy-MM-dd"/>
                                </list>
                            </property>
                        </bean>
                    </list>
                </property>
            </bean>
        </property>
        <property name="fileTypeIdentifier" value="standardAccountExtractFileType"/>
        <property name="fileExtension">
            <value>txt</value>
        </property>
        <property name="directoryPath">
        	<value>${staging.directory}/concur/standardAccountingExtract</value>
        </property>
        <property name="fileNamePrefix" value="extract_CES_SAE_v3_"/>
    </bean>
	
