<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <!--
        NOTE: This override can be removed once cu-kfs upgrades to the 2020-04-09 patch or later.
        If this XML file gets removed, please adjust or remove the "cf.dataDictionaryService"
        bean override from "CUKRADSpringBeans.xml" accordingly.
     -->

    <bean parent="DataDictionaryBeanOverride" p:beanName="Parameter-lookupDefinition">
        <property name="fieldOverrides">
            <list>
                <bean parent="FieldOverrideForListElementReplace" p:propertyName="lookupAttributeDefinitions">
                    <property name="element">
                        <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                              parent="Parameter-componentCode"/>
                    </property>
                    <property name="replaceWith">
                        <bean class="org.kuali.kfs.datadictionary.LookupAttributeDefinition"
                              parent="Parameter-component.name" p:label="Component Name"/>
                    </property>
                </bean>
                <bean parent="FieldOverrideForListElementReplace" p:propertyName="lookupResultAttributeDefinitions">
                    <property name="element">
                        <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                              parent="Parameter-component.name"/>
                    </property>
                    <property name="replaceWith">
                        <bean class="org.kuali.kfs.datadictionary.LookupResultAttributeDefinition"
                              parent="Parameter-component.name" p:label="Component Name"/>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>
