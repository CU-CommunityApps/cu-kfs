<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

    <import resource="classpath:META-INF/cxf/cxf.xml"/>
    <import resource="classpath:META-INF/cxf/cxf-extension-soap.xml"/>
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml"/>

    <jaxws:endpoint
            id="accountVerification"
            implementor="edu.cornell.kfs.coa.service.impl.AccountVerificationWebServiceImpl"
            address="/AccountVerificationWebService"/>

    <jaxws:endpoint
            id="submitTrip"
            implementor="edu.cornell.kfs.fp.service.SubmitTripWebServiceImpl"
            address="/SubmitTripWebService"/>
</beans>