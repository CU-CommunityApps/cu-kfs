    <property name="potentiallySensitive" value="true"/>
    <property name="workflowAttributes">
    	<ref bean="ReceivingDocumentForSearching-workflowAttributes"/>
    </property>  
    <property name="attributes">
      <list>
        
        <ref bean="BulkReceivingDocument-documentNumber"/>
		<ref bean="BulkReceivingDocument-versionNumber"/>
		
        <ref bean="BulkReceivingDocument-purchaseOrderIdentifier"/>
        <ref bean="BulkReceivingDocument-carrierCode"/>
        
        <ref bean="BulkReceivingDocument-shipmentReceivedDate"/>
        <ref bean="BulkReceivingDocument-shipmentPackingSlipNumber"/>
        <ref bean="BulkReceivingDocument-shipmentBillOfLadingNumber"/>
