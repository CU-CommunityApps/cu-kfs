<!-- Attribute Definitions -->

  <bean id="BulkReceivingDocument-documentNumber" parent="BulkReceivingDocument-documentNumber-parentBean"/>

  <bean id="BulkReceivingDocument-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="documentNumber"/>
