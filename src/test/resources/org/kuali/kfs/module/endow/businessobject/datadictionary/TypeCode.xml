<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="TypeCode" parent="TypeCode-parentBean" />

	<bean id="TypeCode-parentBean" abstract="true" parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.module.endow.businessobject.TypeCode" />
		<property name="inquiryDefinition">
			<ref bean="TypeCode-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="TypeCode-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="code" />
		<property name="objectLabel" value="Type Code" />
		<property name="attributes">
			<list>
				<ref bean="TypeCode-code" />
				<ref bean="TypeCode-name" />
				<ref bean="TypeCode-cashSweepModelId" />
				<ref bean="TypeCode-cashSweepModel.cashSweepModelName" />
				<ref bean="TypeCode-incomeACIModelId" />
				<ref
					bean="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.aciModelName" />
				<ref bean="TypeCode-principalACIModelId" />
				<ref
					bean="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.aciModelName" />
				<ref bean="TypeCode-typeFeeMethods.feeMethodCode" />
				<ref bean="TypeCode-typeFeeMethods.active" />
				<ref bean="TypeCode-codeAndDescription" />
				<ref bean="TypeCode-active" />
				<ref bean="TypeCode-cashSweepModel.codeAndDescription" />
				<ref
					bean="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.codeAndDescription" />
				<ref
					bean="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.codeAndDescription" />
				<ref bean="TypeCode-codeForReport" />
			</list>
		</property>
		<property name="collections">
			<list>
				<bean parent="CollectionDefinition" p:name="typeFeeMethods"
					p:label="Type Fee Methods" p:shortLabel="Type Fee Methods"
					p:elementLabel="Type Fee Method" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->

	<bean id="TypeCode-code" parent="TypeCode-code-parentBean" />

	<bean id="TypeCode-code-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="code" />
		<property name="forceUppercase" value="true" />
		<property name="label" value="Type Code" />
		<property name="shortLabel" value="Code" />
		<property name="maxLength" value="3" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" />
		</property>
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="5" />
		</property>
	</bean>
	<bean id="TypeCode-name" parent="TypeCode-name-parentBean" />

	<bean id="TypeCode-name-parentBean" abstract="true" parent="AttributeDefinition">
		<property name="name" value="name" />
		<property name="label" value="Type Code Description" />
		<property name="shortLabel" value="Description" />
		<property name="maxLength" value="100" />
		<property name="validationPattern">
			<bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true" />
		</property>
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="102" />
		</property>
	</bean>
	<bean id="TypeCode-cashSweepModelId" parent="TypeCode-cashSweepModelId-parentBean" />

	<bean id="TypeCode-cashSweepModelId-parentBean" abstract="true"
		parent="CashSweepModel-cashSweepModelID">
		<property name="name" value="cashSweepModelId" />
		<property name="label" value="Default Cash Sweep Model" />
		<property name="maxLength" value="9" />
		<property name="required" value="true" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="11" />
		</property>
	</bean>
	<bean id="TypeCode-cashSweepModel.cashSweepModelName" parent="TypeCode-cashSweepModel.cashSweepModelName-parentBean" />

	<bean id="TypeCode-cashSweepModel.cashSweepModelName-parentBean"
		abstract="true" parent="CashSweepModel-cashSweepModelName">
		<property name="name" value="cashSweepModel.cashSweepModelName" />
		<property name="label" value="Default Cash Sweep Model Description" />
		<property name="shortLabel" value="Cash Sweep Desc" />
	</bean>
	<bean id="TypeCode-incomeACIModelId" parent="TypeCode-incomeACIModelId-parentBean" />

	<bean id="TypeCode-incomeACIModelId-parentBean" abstract="true"
		parent="AutomatedCashInvestmentModel-aciModelID">
		<property name="name" value="incomeACIModelId" />
		<property name="label" value="Default Income ACI Model" />
		<property name="maxLength" value="9" />
		<property name="required" value="true" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="11" />
		</property>
	</bean>
	<bean
		id="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.aciModelName"
		parent="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.aciModelName-parentBean" />

	<bean
		id="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.aciModelName-parentBean"
		abstract="true" parent="AutomatedCashInvestmentModel-aciModelName">
		<property name="name"
			value="automatedCashInvestmentModelForIncomeACIModelId.aciModelName" />
		<property name="label" value="Default Income ACI Model Description" />
		<property name="shortLabel" value="Income ACI Desc" />
	</bean>
	<bean id="TypeCode-principalACIModelId" parent="TypeCode-principalACIModelId-parentBean" />

	<bean id="TypeCode-principalACIModelId-parentBean" abstract="true"
		parent="AutomatedCashInvestmentModel-aciModelID">
		<property name="name" value="principalACIModelId" />
		<property name="label" value="Default Principal ACI Model" />
		<property name="maxLength" value="9" />
		<property name="required" value="true" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="control">
			<bean parent="TextControlDefinition" p:size="11" />
		</property>
	</bean>
	<bean
		id="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.aciModelName"
		parent="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.aciModelName-parentBean" />

	<bean
		id="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.aciModelName-parentBean"
		abstract="true" parent="AutomatedCashInvestmentModel-aciModelName">
		<property name="name"
			value="automatedCashInvestmentModelForPrincipalACIModelId.aciModelName" />
		<property name="label" value="Default Principal ACI Model Description" />
		<property name="shortLabel" value="Principal ACI Desc" />
	</bean>

	<bean id="TypeCode-typeFeeMethods.feeMethodCode" parent="TypeCode-typeFeeMethods.feeMethodCode-parentBean" />

	<bean id="TypeCode-typeFeeMethods.feeMethodCode-parentBean"
		abstract="true" parent="TypeFeeMethod-feeMethodCode">
		<property name="name" value="typeFeeMethods.feeMethodCode" />
		<property name="label" value="Default Fee Method" />
		<property name="shortLabel" value="Fee Mthd" />
	</bean>
	<bean id="TypeCode-typeFeeMethods.active" parent="TypeCode-typeFeeMethods.active-parentBean" />

	<bean id="TypeCode-typeFeeMethods.active-parentBean" abstract="true"
		parent="TypeFeeMethod-active">
		<property name="name" value="typeFeeMethods.active" />
		<property name="label" value="Default Active Indicator" />
		<property name="shortLabel" value="Active" />
	</bean>
	<bean id="TypeCode-codeAndDescription" parent="TypeCode-codeAndDescription-parentBean" />

	<bean id="TypeCode-codeAndDescription-parentBean" abstract="true"
		parent="AttributeDefinition">
		<property name="name" value="codeAndDescription" />
		<property name="label" value="Type Code" />
		<property name="shortLabel" value="Type Code" />
		<property name="maxLength" value="45" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="45" />
		</property>
	</bean>
	<bean id="TypeCode-active" parent="TypeCode-active-parentBean" />

	<bean id="TypeCode-active-parentBean" abstract="true"
		parent="GenericAttributes-activeIndicator">
		<property name="name" value="active" />
	</bean>
	<bean id="TypeCode-cashSweepModel.codeAndDescription" parent="TypeCode-cashSweepModel.codeAndDescription-parentBean" />

	<bean id="TypeCode-cashSweepModel.codeAndDescription-parentBean"
		abstract="true" parent="CashSweepModel-cashSweepModelIDAndDescription">
		<property name="name" value="cashSweepModel.codeAndDescription" />
		<property name="label" value="Default Cash Sweep Model" />
	</bean>
	<bean
		id="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.codeAndDescription"
		parent="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.codeAndDescription-parentBean" />

	<bean
		id="TypeCode-automatedCashInvestmentModelForPrincipalACIModelId.codeAndDescription-parentBean"
		abstract="true" parent="AutomatedCashInvestmentModel-codeAndDescription">
		<property name="name"
			value="automatedCashInvestmentModelForPrincipalACIModelId.codeAndDescription" />
		<property name="label" value="Default Principal ACI Model" />
	</bean>
	<bean
		id="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.codeAndDescription"
		parent="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.codeAndDescription-parentBean" />

	<bean
		id="TypeCode-automatedCashInvestmentModelForIncomeACIModelId.codeAndDescription-parentBean"
		abstract="true" parent="AutomatedCashInvestmentModel-codeAndDescription">
		<property name="name"
			value="automatedCashInvestmentModelForIncomeACIModelId.codeAndDescription" />
		<property name="label" value="Default Income ACI Model" />
	</bean>

	<bean id="TypeCode-codeForReport" parent="TypeCode-codeForReport-parentBean"/>
	<bean id="TypeCode-codeForReport-parentBean" abstract="true" parent="TypeCode-code-parentBean">
		<property name="name" value="codeForReport" />
		<property name="maxLength" value="150" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="100" />
		</property>
	</bean>

	<!-- Business Object Inquiry Definition -->

	<bean id="TypeCode-inquiryDefinition" parent="TypeCode-inquiryDefinition-parentBean" />

	<bean id="TypeCode-inquiryDefinition-parentBean" abstract="true"
		parent="InquiryDefinition">
		<property name="title" value="Type Code Inquiry" />
		<property name="inquirySections">
			<list>
				<ref bean="TypeCode-inquirySectionDefinition-typeCode" />
				<ref bean="TypeCode-inquirySectionDefinition-typeFeeMethod" />
			</list>
		</property>
	</bean>
	<bean id="TypeCode-inquirySectionDefinition-typeCode" parent="TypeCode-inquirySectionDefinition-typeCode-parentBean" />
	<bean id="TypeCode-inquirySectionDefinition-typeCode-parentBean"
		parent="InquirySectionDefinition" abstract="true">
		<property name="title" value="Type Code" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="code" />
				<bean parent="FieldDefinition" p:attributeName="name" />
				<bean parent="FieldDefinition" p:attributeName="cashSweepModel.codeAndDescription" />
				<bean parent="FieldDefinition"
					p:attributeName="automatedCashInvestmentModelForIncomeACIModelId.codeAndDescription" />
				<bean parent="FieldDefinition"
					p:attributeName="automatedCashInvestmentModelForPrincipalACIModelId.codeAndDescription" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>
	<bean id="TypeCode-inquirySectionDefinition-typeFeeMethod"
		parent="TypeCode-inquirySectionDefinition-typeFeeMethod-parentBean" />
	<bean id="TypeCode-inquirySectionDefinition-typeFeeMethod-parentBean"
		parent="InquirySectionDefinition" abstract="true">
		<property name="title" value="Type Fee Method" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<ref bean="TypeCode-inquiryCollectionDefinition-typeFeeMethods" />
			</list>
		</property>
	</bean>
	<bean id="TypeCode-inquiryCollectionDefinition-typeFeeMethods"
		parent="TypeCode-inquiryCollectionDefinition-typeFeeMethods-parentBean" />
	<bean id="TypeCode-inquiryCollectionDefinition-typeFeeMethods-parentBean"
		parent="InquiryCollectionDefinition" abstract="true">
		<property name="attributeName" value="typeFeeMethods" />
		<property name="businessObjectClass"
			value="org.kuali.kfs.module.endow.businessobject.TypeFeeMethod" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="feeMethodCode" />
				<bean parent="FieldDefinition" p:attributeName="feeMethod.name" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->

	<bean id="TypeCode-lookupDefinition" parent="TypeCode-lookupDefinition-parentBean" />

	<bean id="TypeCode-lookupDefinition-parentBean" abstract="true"
		parent="LookupDefinition">
		<property name="title" value="Type Code Lookup" />

		<property name="defaultSort">
			<bean parent="SortDefinition">
				<property name="attributeNames">
					<list>
						<value>code</value>
					</list>
				</property>
			</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="code" />
				<bean parent="FieldDefinition" p:attributeName="name" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="code" />
				<bean parent="FieldDefinition" p:attributeName="name" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>
</beans>
