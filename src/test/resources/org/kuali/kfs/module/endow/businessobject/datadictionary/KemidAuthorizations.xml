<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         
			http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         
			http://rice.kuali.org/dd         
			http://rice.kuali.org/dd/dd.xsd">

	<bean id="KemidAuthorizations" parent="KemidAuthorizations-parentBean" />

	<bean id="KemidAuthorizations-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.module.endow.businessobject.KemidAuthorizations" />
		<property name="inquiryDefinition">
			<ref bean="KemidAuthorizations-inquiryDefinition" />
		</property>
		<property name="titleAttribute" value="benefittingOrgSeqNumber" />
		<property name="objectLabel" value="Kemid Authorizations" />
		<property name="attributes">
			<list>
				<ref bean="KemidAuthorizations-kemid" />
				<ref bean="KemidAuthorizations-roleSequenceNumber" />
				<ref bean="KemidAuthorizations-roleId" />
				<ref bean="KemidAuthorizations-rolePrincipalId" />
				<ref bean="KemidAuthorizations-rolePrincipalIdForSearching" />
				<ref bean="KemidAuthorizations-rolePrincipal.principalName" />
				<ref bean="KemidAuthorizations-rolePrincipal.name" />
				<ref bean="KemidAuthorizations-role.name" />
				<ref bean="KemidAuthorizations-roleTerminationDate" />
				<ref bean="KemidAuthorizations-active" />
				<ref bean="KemidAuthorizations-newCollectionRecord" />
			</list>
		</property>
		<property name="relationships">
			<list>
				<bean parent="RelationshipDefinition" p:objectAttributeName="rolePrincipal">
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="rolePrincipalId" p:targetName="principalId" />
						</list>
					</property>
					<property name="supportAttributes">
						<list>
							<bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="rolePrincipal.principalName" p:targetName="principalName" />
							<bean parent="SupportAttributeDefinition" p:sourceName="rolePrincipal.name" p:targetName="name" />
						</list>
					</property>
				</bean>
				<bean parent="RelationshipDefinition" p:objectAttributeName="role">
					<property name="primitiveAttributes">
						<list>
							<bean parent="PrimitiveAttributeDefinition" p:sourceName="roleId" p:targetName="id" />
						</list>
					</property>
					<property name="supportAttributes">
						<list>
							<bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="role.id" p:targetName="id" />
							<bean parent="SupportAttributeDefinition" p:sourceName="role.name" p:targetName="name" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>


	<!-- Attribute Definitions -->

	<bean id="KemidAuthorizations-kemid" parent="KemidAuthorizations-kemid-parentBean" />

	<bean id="KemidAuthorizations-kemid-parentBean" abstract="true"
		parent="KEMID-kemid">
		<property name="name" value="kemid" />
		<property name="required" value="true" />
	</bean>
	<bean id="KemidAuthorizations-roleSequenceNumber" parent="KemidAuthorizations-roleSequenceNumber-parentBean" />

	<bean id="KemidAuthorizations-roleSequenceNumber-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="roleSequenceNumber" />
		<property name="label" value="Authorization Sequence Number" />
		<property name="shortLabel" value="Auth Seq Number" />
		<property name="maxLength" value="4" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="5" />
		</property>
	</bean>
	<bean id="KemidAuthorizations-roleId" parent="KemidAuthorizations-roleId-parentBean" />

	<bean id="KemidAuthorizations-roleId-parentBean" abstract="true"
		parent="RoleBo-id">
		<property name="name" value="roleId" />
		<property name="label" value="Role ID" />
		<property name="shortLabel" value="Role ID" />
	</bean>
	<bean id="KemidAuthorizations-rolePrincipalId" parent="KemidAuthorizations-rolePrincipalId-parentBean" />

	<bean id="KemidAuthorizations-rolePrincipalId-parentBean"
		abstract="true" parent="PersonImpl-principalId">
		<property name="name" value="rolePrincipalId" />
		<property name="label" value="Principal ID" />
		<property name="shortLabel" value="Principal ID" />
		<property name="control">
			<ref bean="HiddenControl" />
		</property>
		<property name="displayLabelAttribute" value="rolePrincipal.principalName" />
	</bean>
	<bean id="KemidAuthorizations-rolePrincipalIdForSearching"
		parent="KemidAuthorizations-rolePrincipalIdForSearching-parentBean" />

	<bean id="KemidAuthorizations-rolePrincipalIdForSearching-parentBean"
		abstract="true" parent="PersonImpl-principalId">
		<property name="name" value="rolePrincipalIdForSearching" />
		<property name="label" value="Principal ID" />
		<property name="shortLabel" value="Princ ID" />
		<property name="displayLabelAttribute" value="rolePrincipal.principalName" />
	</bean>
	<bean id="KemidAuthorizations-rolePrincipal.principalName"
		parent="KemidAuthorizations-rolePrincipal.principalName-parentBean" />

	<bean id="KemidAuthorizations-rolePrincipal.principalName-parentBean"
		parent="PersonImpl-principalName" abstract="true"
		p:name="rolePrincipal.principalName" p:label="Role Principal Name"
		p:shortLabel=" Role Principal Name" p:required="false">
		<property name="control">
			<bean parent="KualiUserControlDefinition"
				p:universalIdAttributeName="rolePrincipalId" p:userIdAttributeName="rolePrincipal" />
		</property>
	</bean>
	<bean id="KemidAuthorizations-rolePrincipal.name" parent="KemidAuthorizations-rolePrincipal.name-parentBean" />

	<bean id="KemidAuthorizations-rolePrincipal.name-parentBean"
		abstract="true" parent="PersonImpl-name">
		<property name="name" value="rolePrincipal.name" />
		<property name="label" value="Role Principal Name" />
		<property name="required" value="false" />
	</bean>
	<bean id="KemidAuthorizations-role.name" parent="KemidAuthorizations-role.name-parentBean" />

	<bean id="KemidAuthorizations-role.name-parentBean" abstract="true" parent="RoleBo-name">
		<property name="name" value="role.name" />
	</bean>
	<bean id="KemidAuthorizations-roleTerminationDate" parent="KemidAuthorizations-roleTerminationDate-parentBean" />

	<bean id="KemidAuthorizations-roleTerminationDate-parentBean"
		abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="roleTerminationDate" />
		<property name="label" value="Role Termination Date" />
	</bean>
	<bean id="KemidAuthorizations-active" parent="KemidAuthorizations-active-parentBean" />

	<bean id="KemidAuthorizations-active-parentBean" abstract="true"
		parent="GenericAttributes-genericBoolean">
		<property name="name" value="active" />
		<property name="label" value="Active" />
	</bean>
	<bean id="KemidAuthorizations-newCollectionRecord" parent="KemidAuthorizations-newCollectionRecord-parentBean" />

	<bean id="KemidAuthorizations-newCollectionRecord-parentBean"
		abstract="true" parent="GenericAttributes-newCollectionRecord">
	</bean>

	<!-- Business Object Inquiry Definition -->

	<bean id="KemidAuthorizations-inquiryDefinition" parent="KemidAuthorizations-inquiryDefinition-parentBean" />

	<bean id="KemidAuthorizations-inquiryDefinition-parentBean"
		abstract="true" parent="InquiryDefinition">
		<property name="title" value="Benefitting Organization Inquiry" />
		<property name="inquirySections">
			<list>
				<ref bean="KemidAuthorizations-inquirySectionDefinition" />
			</list>
		</property>
	</bean>
	<bean id="KemidAuthorizations-inquirySectionDefinition" parent="KemidAuthorizations-inquirySectionDefinition-parentBean" />
	<bean id="KemidAuthorizations-inquirySectionDefinition-parentBean"
		parent="InquirySectionDefinition" abstract="true">
		<property name="title" value="" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="kemid" />
				<bean parent="FieldDefinition" p:attributeName="roleSequenceNumber" />
				<bean parent="FieldDefinition" p:attributeName="roleId" />
				<bean parent="FieldDefinition" p:attributeName="role.name" />
				<bean parent="FieldDefinition" p:attributeName="rolePrincipalId" />
				<bean parent="FieldDefinition" p:attributeName="roleTerminationDate" />
				<bean parent="FieldDefinition" p:attributeName="active" />
			</list>
		</property>
	</bean>

</beans>