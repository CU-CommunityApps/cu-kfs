<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation="http://www.springframework.org/schema/beans         
			http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         
			http://rice.kuali.org/dd         
			http://rice.kuali.org/dd/dd.xsd">

	<bean id="KemidCombineDonorStatement" parent="KemidCombineDonorStatement-parentBean" />

	<bean id="KemidCombineDonorStatement-parentBean" abstract="true"
		parent="BusinessObjectEntry">
		<property name="businessObjectClass"
			value="org.kuali.kfs.module.endow.businessobject.KemidCombineDonorStatement" />
		<property name="inquiryDefinition">
			<ref bean="KemidCombineDonorStatement-inquiryDefinition" />
		</property>
		<property name="lookupDefinition">
			<ref bean="KemidCombineDonorStatement-lookupDefinition" />
		</property>
		<property name="titleAttribute" value="combineDonorSeq" />
		<property name="objectLabel" value="Combine Donor Statement" />
		<property name="attributes">
			<list>
				<ref bean="KemidCombineDonorStatement-kemid" />
				<ref bean="KemidCombineDonorStatement-combineDonorSeq" />
				<ref bean="KemidCombineDonorStatement-combineWithKemid" />
				<ref bean="KemidCombineDonorStatement-combineWithKemidObjRef.shortTitle" />
				<ref
					bean="KemidCombineDonorStatement-combineWithKemidObjRef.kemidAndShortTitle" />
				<ref bean="KemidCombineDonorStatement-combineDate" />
				<ref bean="KemidCombineDonorStatement-terminateCombineDate" />
				<ref bean="KemidCombineDonorStatement-newCollectionRecord" />
			</list>
		</property>
	</bean>

	<!-- Attribute Definitions -->

	<bean id="KemidCombineDonorStatement-kemid" parent="KemidCombineDonorStatement-kemid-parentBean" />

	<bean id="KemidCombineDonorStatement-kemid-parentBean" abstract="true"
		parent="KEMID-kemid">
		<property name="name" value="kemid" />
		<property name="required" value="true" />
	</bean>
	<bean id="KemidCombineDonorStatement-combineDonorSeq" parent="KemidCombineDonorStatement-combineDonorSeq-parentBean" />

	<bean id="KemidCombineDonorStatement-combineDonorSeq-parentBean"
		abstract="true" parent="AttributeDefinition">
		<property name="name" value="combineDonorSeq" />
		<property name="label" value="Combine Donor Sequence Number" />
		<property name="shortLabel" value="Combine Donor Seq Number" />
		<property name="maxLength" value="3" />
		<property name="validationPattern">
			<bean parent="NumericValidationPattern" />
		</property>
		<property name="required" value="true" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="5" />
		</property>
	</bean>
	<bean id="KemidCombineDonorStatement-combineWithKemid" parent="KemidCombineDonorStatement-combineWithKemid-parentBean" />

	<bean id="KemidCombineDonorStatement-combineWithKemid-parentBean"
		abstract="true" parent="KEMID-kemid">
		<property name="name" value="combineWithKemid" />
		<property name="label" value="Combine with KEMID" />
		<property name="required" value="true" />
	</bean>
	<bean id="KemidCombineDonorStatement-combineWithKemidObjRef.shortTitle"
		parent="KemidCombineDonorStatement-combineWithKemidObjRef.shortTitle-parentBean" />

	<bean
		id="KemidCombineDonorStatement-combineWithKemidObjRef.shortTitle-parentBean"
		abstract="true" parent="KEMID-shortTitle">
		<property name="name" value="combineWithKemidObjRef.shortTitle" />
		<property name="label" value="Combine with KEMID Short Title" />
	</bean>
	<bean
		id="KemidCombineDonorStatement-combineWithKemidObjRef.kemidAndShortTitle"
		parent="KemidCombineDonorStatement-combineWithKemidObjRef.kemidAndShortTitle-parentBean" />

	<bean
		id="KemidCombineDonorStatement-combineWithKemidObjRef.kemidAndShortTitle-parentBean"
		abstract="true" parent="KEMID-kemidAndShortTitle">
		<property name="name" value="combineWithKemidObjRef.kemidAndShortTitle" />
		<property name="label" value="Combine with KEMID" />
	</bean>
	<bean id="KemidCombineDonorStatement-combineDate" parent="KemidCombineDonorStatement-combineDate-parentBean" />

	<bean id="KemidCombineDonorStatement-combineDate-parentBean"
		abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="combineDate" />
		<property name="label" value="Combine Date" />
	</bean>
	<bean id="KemidCombineDonorStatement-terminateCombineDate"
		parent="KemidCombineDonorStatement-terminateCombineDate-parentBean" />

	<bean id="KemidCombineDonorStatement-terminateCombineDate-parentBean"
		abstract="true" parent="GenericAttributes-genericDate">
		<property name="name" value="terminateCombineDate" />
		<property name="label" value="Terminate Combine Date" />
	</bean>
	<bean id="KemidCombineDonorStatement-newCollectionRecord"
		parent="KemidCombineDonorStatement-newCollectionRecord-parentBean" />

	<bean id="KemidCombineDonorStatement-newCollectionRecord-parentBean"
		abstract="true" parent="GenericAttributes-newCollectionRecord">
	</bean>

	<!-- Business Object Inquiry Definition -->

	<bean id="KemidCombineDonorStatement-inquiryDefinition" parent="KemidCombineDonorStatement-inquiryDefinition-parentBean" />

	<bean id="KemidCombineDonorStatement-inquiryDefinition-parentBean"
		abstract="true" parent="InquiryDefinition">
		<property name="title" value="Combine Donor Statement Inquiry" />
		<property name="inquirySections">
			<list>
				<ref bean="KemidCombineDonorStatement-inquirySectionDefinition" />
			</list>
		</property>
	</bean>
	<bean id="KemidCombineDonorStatement-inquirySectionDefinition"
		parent="KemidCombineDonorStatement-inquirySectionDefinition-parentBean" />
	<bean id="KemidCombineDonorStatement-inquirySectionDefinition-parentBean"
		parent="InquirySectionDefinition" abstract="true">
		<property name="title" value="" />
		<property name="numberOfColumns" value="1" />
		<property name="inquiryFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="kemid" />
				<bean parent="FieldDefinition" p:attributeName="combineDonorSeq" />
				<bean parent="FieldDefinition" p:attributeName="combineWithKemid" />
				<bean parent="FieldDefinition" p:attributeName="combineDate" />
				<bean parent="FieldDefinition" p:attributeName="terminateCombineDate" />
			</list>
		</property>
	</bean>

	<!-- Business Object Lookup Definition -->

	<bean id="KemidCombineDonorStatement-lookupDefinition" parent="KemidCombineDonorStatement-lookupDefinition-parentBean" />

	<bean id="KemidCombineDonorStatement-lookupDefinition-parentBean"
		abstract="true" parent="LookupDefinition">
		<property name="title" value="Combine Donor Statement Lookup" />
		<property name="defaultSort">
			<bean parent="SortDefinition">
				<property name="attributeNames">
					<list>
						<value>kemid</value>
						<value>combineDonorSeq</value>
					</list>
				</property>
			</bean>
		</property>
		<property name="lookupFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="kemid" />
				<bean parent="FieldDefinition" p:attributeName="combineDonorSeq" />
				<bean parent="FieldDefinition" p:attributeName="combineWithKemid" />
			</list>
		</property>
		<property name="resultFields">
			<list>
				<bean parent="FieldDefinition" p:attributeName="kemid" />
				<bean parent="FieldDefinition" p:attributeName="combineDonorSeq" />
				<bean parent="FieldDefinition" p:attributeName="combineWithKemid" />
			</list>
		</property>
	</bean>

</beans>